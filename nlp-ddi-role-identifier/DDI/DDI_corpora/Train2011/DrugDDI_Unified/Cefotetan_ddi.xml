<document id="DrugDDI.d54" origId="Cefotetan"><sentence id="DrugDDI.d54.s0" origId="s0" text="Increases in serum creatinine have occurred when CEFOTAN was given alone."><entity id="DrugDDI.d54.s0.e0" origId="s0.p5" charOffset="49-56" type="drug" text="CEFOTAN"/></sentence><sentence id="DrugDDI.d54.s1" origId="s1" text="If CEFOTAN and an aminoglycoside are used concomitantly, renal function should be carefully monitored, because nephrotoxicity may be potentiated."><entity id="DrugDDI.d54.s1.e0" origId="s1.p9" charOffset="3-10" type="drug" text="CEFOTAN"/><entity id="DrugDDI.d54.s1.e1" origId="s1.p11" charOffset="18-32" type="drug" text="aminoglycoside"/><pair id="DrugDDI.d54.s1.p0" e1="DrugDDI.d54.s1.e0" e2="DrugDDI.d54.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d54.s2" origId="s2" text="Drug/Laboratory Test Interactions: The administration of CEFOTAN may result in a false positive reaction for glucose in the urine using Clinitest??, Benedicts solution, or Fehlings solution."><entity id="DrugDDI.d54.s2.e0" origId="s2.p26" charOffset="0-4" type="drug" text="Drug"/><entity id="DrugDDI.d54.s2.e1" origId="s2.p30" charOffset="57-64" type="drug" text="CEFOTAN"/><entity id="DrugDDI.d54.s2.e2" origId="s2.p34" charOffset="109-116" type="drug" text="glucose"/><pair id="DrugDDI.d54.s2.p0" e1="DrugDDI.d54.s2.e0" e2="DrugDDI.d54.s2.e1" interaction="false"/><pair id="DrugDDI.d54.s2.p1" e1="DrugDDI.d54.s2.e0" e2="DrugDDI.d54.s2.e2" interaction="false"/><pair id="DrugDDI.d54.s2.p2" e1="DrugDDI.d54.s2.e1" e2="DrugDDI.d54.s2.e2" interaction="false"/></sentence><sentence id="DrugDDI.d54.s3" origId="s3" text="It is recommended that glucose tests based on enzymatic glucose oxidase be used."><entity id="DrugDDI.d54.s3.e0" origId="s3.p50" charOffset="56-71" type="drug" text="glucose oxidase"/></sentence><sentence id="DrugDDI.d54.s4" origId="s4" text="As with other cephalosporins, high concentrations of cefotetan may interfere with measurement of serum and urine creatinine levels by Jaffe? reaction and produce false increases in the levels of creatinine reported."><entity id="DrugDDI.d54.s4.e0" origId="s4.p54" charOffset="14-28" type="drug" text="cephalosporins"/><entity id="DrugDDI.d54.s4.e1" origId="s4.p56" charOffset="53-62" type="drug" text="cefotetan"/><entity id="DrugDDI.d54.s4.e2" origId="s4.p67" charOffset="195-205" type="drug" text="creatinine"/><pair id="DrugDDI.d54.s4.p0" e1="DrugDDI.d54.s4.e0" e2="DrugDDI.d54.s4.e1" interaction="false"/><pair id="DrugDDI.d54.s4.p1" e1="DrugDDI.d54.s4.e0" e2="DrugDDI.d54.s4.e2" interaction="false"/><pair id="DrugDDI.d54.s4.p2" e1="DrugDDI.d54.s4.e1" e2="DrugDDI.d54.s4.e2" interaction="true"/></sentence></document>