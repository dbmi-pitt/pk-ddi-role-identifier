<document id="DrugDDI.d42" origId="Enflurane"><sentence id="DrugDDI.d42.s0" origId="s0" text="The action of nondepolarizing relaxants is augmented by Enflurane."><entity id="DrugDDI.d42.s0.e0" origId="s0.p4" charOffset="56-65" type="drug" text="Enflurane"/></sentence><sentence id="DrugDDI.d42.s1" origId="s1" text="Less than the usual amounts of these medicines should be used."><entity id="DrugDDI.d42.s1.e0" origId="s1.p7" charOffset="37-46" type="drug" text="medicines"/></sentence><sentence id="DrugDDI.d42.s2" origId="s2" text="If the usual amounts of nondepolarizing relaxants are given, the time for recovery from neuromuscular blockade will be longer in the presence of Enflurane than when halothane or nitrous oxide with a balanced technique are used."><entity id="DrugDDI.d42.s2.e0" origId="s2.p23" charOffset="145-154" type="drug" text="Enflurane"/><entity id="DrugDDI.d42.s2.e1" origId="s2.p26" charOffset="165-174" type="drug" text="halothane"/><entity id="DrugDDI.d42.s2.e2" origId="s2.p28" charOffset="178-191" type="drug" text="nitrous oxide"/><pair id="DrugDDI.d42.s2.p0" e1="DrugDDI.d42.s2.e0" e2="DrugDDI.d42.s2.e1" interaction="false"/><pair id="DrugDDI.d42.s2.p1" e1="DrugDDI.d42.s2.e0" e2="DrugDDI.d42.s2.e2" interaction="false"/><pair id="DrugDDI.d42.s2.p2" e1="DrugDDI.d42.s2.e1" e2="DrugDDI.d42.s2.e2" interaction="false"/></sentence></document>