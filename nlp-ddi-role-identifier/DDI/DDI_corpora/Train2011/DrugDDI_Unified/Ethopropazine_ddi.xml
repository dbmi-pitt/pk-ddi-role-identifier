<document id="DrugDDI.d136" origId="Ethopropazine"><sentence id="DrugDDI.d136.s0" origId="s0" text="Ethopropazine may interact with alcohol or other CNS depressants, causing increased sedative effects."><entity id="DrugDDI.d136.s0.e0" origId="s0.p0" charOffset="0-13" type="drug" text="Ethopropazine"/><entity id="DrugDDI.d136.s0.e1" origId="s0.p3" charOffset="32-39" type="drug" text="alcohol"/><entity id="DrugDDI.d136.s0.e2" origId="s0.p5" charOffset="49-64" type="drug" text="CNS depressants"/><entity id="DrugDDI.d136.s0.e3" origId="s0.p7" charOffset="84-92" type="drug" text="sedative"/><pair id="DrugDDI.d136.s0.p0" e1="DrugDDI.d136.s0.e0" e2="DrugDDI.d136.s0.e1" interaction="true"/><pair id="DrugDDI.d136.s0.p1" e1="DrugDDI.d136.s0.e0" e2="DrugDDI.d136.s0.e2" interaction="true"/><pair id="DrugDDI.d136.s0.p2" e1="DrugDDI.d136.s0.e0" e2="DrugDDI.d136.s0.e3" interaction="false"/><pair id="DrugDDI.d136.s0.p3" e1="DrugDDI.d136.s0.e1" e2="DrugDDI.d136.s0.e2" interaction="false"/><pair id="DrugDDI.d136.s0.p4" e1="DrugDDI.d136.s0.e1" e2="DrugDDI.d136.s0.e3" interaction="false"/><pair id="DrugDDI.d136.s0.p5" e1="DrugDDI.d136.s0.e2" e2="DrugDDI.d136.s0.e3" interaction="false"/></sentence><sentence id="DrugDDI.d136.s1" origId="s1" text="It may also interact with amantadine or other anticholinergic drugs or MAOIs, which may intensify the anticholinergic action."><entity id="DrugDDI.d136.s1.e0" origId="s1.p12" charOffset="26-36" type="drug" text="amantadine"/><entity id="DrugDDI.d136.s1.e1" origId="s1.p14" charOffset="46-67" type="drug" text="anticholinergic drugs"/><entity id="DrugDDI.d136.s1.e2" origId="s1.p20" charOffset="102-117" type="drug" text="anticholinergic"/><pair id="DrugDDI.d136.s1.p0" e1="DrugDDI.d136.s1.e0" e2="DrugDDI.d136.s1.e1" interaction="false"/><pair id="DrugDDI.d136.s1.p1" e1="DrugDDI.d136.s1.e0" e2="DrugDDI.d136.s1.e2" interaction="false"/><pair id="DrugDDI.d136.s1.p2" e1="DrugDDI.d136.s1.e1" e2="DrugDDI.d136.s1.e2" interaction="false"/></sentence><sentenc