<document id="DrugDDI.d431" origId="Griseofulvin"><sentence id="DrugDDI.d431.s0" origId="s0" text="Patients on warfarin-type anticoagulant therapy may require dosage adjustment of the anticoagulant during and after griseofulvin therapy."><entity id="DrugDDI.d431.s0.e0" origId="s0.p1" charOffset="12-20" type="drug" text="warfarin"/><entity id="DrugDDI.d431.s0.e1" origId="s0.p5" charOffset="85-98" type="drug" text="anticoagulant"/><entity id="DrugDDI.d431.s0.e2" origId="s0.p8" charOffset="116-128" type="drug" text="griseofulvin"/><pair id="DrugDDI.d431.s0.p0" e1="DrugDDI.d431.s0.e0" e2="DrugDDI.d431.s0.e1" interaction="false"/><pair id="DrugDDI.d431.s0.p1" e1="DrugDDI.d431.s0.e0" e2="DrugDDI.d431.s0.e2" interaction="true"/><pair id="DrugDDI.d431.s0.p2" e1="DrugDDI.d431.s0.e1" e2="DrugDDI.d431.s0.e2" interaction="false"/></sentence><sentence id="DrugDDI.d431.s1" origId="s1" text="Concomitant use of barbiturates usually depresses griseofulvin activity and may necessitate raising the dosage."><entity id="DrugDDI.d431.s1.e0" origId="s1.p10" charOffset="19-31" type="drug" text="barbiturates"/><entity id="DrugDDI.d431.s1.e1" origId="s1.p12" charOffset="50-62" type="drug" text="griseofulvin"/><pair id="DrugDDI.d431.s1.p0" e1="DrugDDI.d431.s1.e0" e2="DrugDDI.d431.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d431.s2" origId="s2" text="The concomitant administration of griseofulvin has been reported to reduce the efficacy of oral contraceptives and to increase the incidence of breakthrough bleeding."><entity id="DrugDDI.d431.s2.e0" origId="s2.p19" charOffset="34-46" type="drug" text="griseofulvin"/><entity id="DrugDDI.d431.s2.e1" origId="s2.p26" charOffset="91-110" type="drug" text="oral contraceptives"/><pair id="DrugDDI.d431.s2.p0" e1="DrugDDI.d431.s2.e0" e2="DrugDDI.d431.s2.e1" interaction="true"/></sentence></document>