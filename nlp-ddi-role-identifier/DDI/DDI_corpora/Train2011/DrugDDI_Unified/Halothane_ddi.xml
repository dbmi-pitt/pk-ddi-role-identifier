<document id="DrugDDI.d90" origId="Halothane"><sentence id="DrugDDI.d90.s0" origId="s0" text="FLUOTHANE augments the action of non-depolarising muscle relaxants and the muscle relaxant effects of aminoglycosides."><entity id="DrugDDI.d90.s0.e0" origId="s0.p0" charOffset="0-9" type="drug" text="FLUOTHANE"/><entity id="DrugDDI.d90.s0.e1" origId="s0.p3" charOffset="33-65" type="drug" text="non-depolarising muscle relaxant"/><entity id="DrugDDI.d90.s0.e2" origId="s0.p5" charOffset="75-90" type="drug" text="muscle relaxant"/><entity id="DrugDDI.d90.s0.e3" origId="s0.p6" charOffset="102-117" type="drug" text="aminoglycosides"/><pair id="DrugDDI.d90.s0.p0" e1="DrugDDI.d90.s0.e0" e2="DrugDDI.d90.s0.e1" interaction="true"/><pair id="DrugDDI.d90.s0.p1" e1="DrugDDI.d90.s0.e0" e2="DrugDDI.d90.s0.e2" interaction="false"/><pair id="DrugDDI.d90.s0.p2" e1="DrugDDI.d90.s0.e0" e2="DrugDDI.d90.s0.e3" interaction="false"/><pair id="DrugDDI.d90.s0.p3" e1="DrugDDI.d90.s0.e1" e2="DrugDDI.d90.s0.e2" interaction="false"/><pair id="DrugDDI.d90.s0.p4" e1="DrugDDI.d90.s0.e1" e2="DrugDDI.d90.s0.e3" interaction="false"/><pair id="DrugDDI.d90.s0.p5" e1="DrugDDI.d90.s0.e2" e2="DrugDDI.d90.s0.e3" interaction="false"/></sentence><sentence id="DrugDDI.d90.s1" origId="s1" text="FLUOTHANE may augment the hypotension caused by the ganglionic-blocking effect of tubocurarine."><entity id="DrugDDI.d90.s1.e0" origId="s1.p7" charOffset="0-9" type="drug" text="FLUOTHANE"/><entity id="DrugDDI.d90.s1.e1" origId="s1.p13" charOffset="82-94" type="drug" text="tubocurarine"/><pair id="DrugDDI.d90.s1.p0" e1="DrugDDI.d90.s1.e0" e2="DrugDDI.d90.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d90.s2" origId="s2" text="Caution should be exercised during the administration of adrenaline to patients anaesthetised with FLUOTHANE as arrhythmias may be precipitated."><entity id="DrugDDI.d90.s2.e0" origId="s2.p19" charOffset="57-67" type="drug" text="adrenaline"/><entity id="DrugDDI.d90.s2.e1" origId="s2.p22" charOffset="99-108" type="drug" text="FLUOTHANE"/><pair id="DrugDDI.d90.s2.p0" e1="DrugDDI.d90.s2.e0" e2="DrugDDI.d90.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d90.s3" origId="s3" text="For this reason the dose of adrenaline should be restricted and an antiarrhythmic agent administered as appropriate."><entity id="DrugDDI.d90.s3.e0" origId="s3.p30" charOffset="28-38" type="drug" text="adrenaline"/><entity id="DrugDDI.d90.s3.e1" origId="s3.p35" charOffset="67-87" type="drug" text="antiarrhythmic agent"/><pair id="DrugDDI.d90.s3.p0" e1="DrugDDI.d90.s3.e0" e2="DrugDDI.d90.s3.e1" interaction="false"/></sentence><sentence id="DrugDDI.d90.s4" origId="s4" text="Caution should also be applied for other sympathomimetics, and for aminophylline and theophylline and tricyclic antidepressants, which may also precipitate arrhythmias."><entity id="DrugDDI.d90.s4.e0" origId="s4.p43" charOffset="41-57" type="drug" text="sympathomimetics"/><entity id="DrugDDI.d90.s4.e1" origId="s4.p45" charOffset="67-80" type="drug" text="aminophylline"/><entity id="DrugDDI.d90.s4.e2" origId="s4.p47" charOffset="85-97" type="drug" text="theophylline"/><entity id="DrugDDI.d90.s4.e3" origId="s4.p49" charOffset="102-127" type="drug" text="tricyclic antidepressants"/><pair id="DrugDDI.d90.s4.p0" e1="DrugDDI.d90.s4.e0" e2="DrugDDI.d90.s4.e1" interaction="false"/><pair id="DrugDDI.d90.s4.p1" e1="DrugDDI.d90.s4.e0" e2="DrugDDI.d90.s4.e2" interaction="false"/><pair id="DrugDDI.d90.s4.p2" e1="DrugDDI.d90.s4.e0" e2="DrugDDI.d90.s4.e3" interaction="false"/><pair id="DrugDDI.d90.s4.p3" e1="DrugDDI.d90.s4.e1" e2="DrugDDI.d90.s4.e2" interaction="false"/><pair id="DrugDDI.d90.s4.p4" e1="DrugDDI.d90.s4.e1" e2="DrugDDI.d90.s4.e3" interaction="false"/><pair id="DrugDDI.d90.s4.p5" e1="DrugDDI.d90.s4.e2" e2="DrugDDI.d90.s4.e3" interaction="false"/></sentence></document>