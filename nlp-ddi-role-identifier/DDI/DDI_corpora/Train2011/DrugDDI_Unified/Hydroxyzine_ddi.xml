<document id="DrugDDI.d474" origId="Hydroxyzine"><sentence id="DrugDDI.d474.s0" origId="s0" text="THE POTENTIATING ACTION OF HYDROXYZINE MUST BE CONSIDERED WHEN THE DRUG IS USED IN CONJUNCTION WITH CENTRAL NERVOUS SYSTEM DEPRESSANTS SUCH AS NARCOTICS, NON-NARCOTIC ANALGESICS AND BARBITURATES."><entity id="DrugDDI.d474.s0.e0" origId="s0.p1" charOffset="27-38" type="drug" text="HYDROXYZINE"/><entity id="DrugDDI.d474.s0.e1" origId="s0.p6" charOffset="67-71" type="drug" text="DRUG"/><entity id="DrugDDI.d474.s0.e2" origId="s0.p9" charOffset="100-134" type="drug" text="CENTRAL NERVOUS SYSTEM DEPRESSANTS"/><entity id="DrugDDI.d474.s0.e3" origId="s0.p10" charOffset="143-152" type="drug" text="NARCOTICS"/><entity id="DrugDDI.d474.s0.e4" origId="s0.p11" charOffset="154-178" type="drug" text="NON-NARCOTIC ANALGESICS"/><entity id="DrugDDI.d474.s0.e5" origId="s0.p13" charOffset="182-194" type="drug" text="BARBITURATES"/><pair id="DrugDDI.d474.s0.p0" e1="DrugDDI.d474.s0.e0" e2="DrugDDI.d474.s0.e1" interaction="false"/><pair id="DrugDDI.d474.s0.p1" e1="DrugDDI.d474.s0.e0" e2="DrugDDI.d474.s0.e2" interaction="true"/><pair id="DrugDDI.d474.s0.p2" e1="DrugDDI.d474.s0.e0" e2="DrugDDI.d474.s0.e3" interaction="false"/><pair id="DrugDDI.d474.s0.p3" e1="DrugDDI.d474.s0.e0" e2="DrugDDI.d474.s0.e4" interaction="true"/><pair id="DrugDDI.d474.s0.p4" e1="DrugDDI.d474.s0.e0" e2="DrugDDI.d474.s0.e5" interaction="false"/><pair id="DrugDDI.d474.s0.p5" e1="DrugDDI.d474.s0.e1" e2="DrugDDI.d474.s0.e2" interaction="false"/><pair id="DrugDDI.d474.s0.p6" e1="DrugDDI.d474.s0.e1" e2="DrugDDI.d474.s0.e3" interaction="false"/><pair id="DrugDDI.d474.s0.p7" e1="DrugDDI.d474.s0.e1" e2="DrugDDI.d474.s0.e4" interaction="false"/><pair id="DrugDDI.d474.s0.p8" e1="DrugDDI.d474.s0.e1" e2="DrugDDI.d474.s0.e5" interaction="false"/><pair id="DrugDDI.d474.s0.p9" e1="DrugDDI.d474.s0.e2" e2="DrugDDI.d474.s0.e3" interaction="false"/><pair id="DrugDDI.d474.s0.p10" e1="DrugDDI.d474.s0.e2" e2="DrugDDI.d474.s0.e4" interaction="false"/><pair id="DrugDDI.d474.s0.p11" e1="DrugDDI.d474.s0.e2" e2="DrugDDI.d474.s0.e5" interaction="false"/><pair id="DrugDDI.d474.s0.p12" e1="DrugDDI.d474.s0.e3" e2="DrugDDI.d474.s0.e4" interaction="false"/><pair id="DrugDDI.d474.s0.p13" e1="DrugDDI.d474.s0.e3" e2="DrugDDI.d474.s0.e5" interaction="false"/><pair id="DrugDDI.d474.s0.p14" e1="DrugDDI.d474.s0.e4" e2="DrugDDI.d474.s0.e5" interaction="false"/></sentence><sentence id="DrugDDI.d474.s1" origId="s1" text="Therefore when central nervous system depressants are administered concomitantly with hydroxyzine their dosage should be reduced."><entity id="DrugDDI.d474.s1.e0" origId="s1.p16" charOffset="15-49" type="drug" text="central nervous system depressants"/><entity id="DrugDDI.d474.s1.e1" origId="s1.p20" charOffset="86-97" type="drug" text="hydroxyzine"/><pair id="DrugDDI.d474.s1.p0" e1="DrugDDI.d474.s1.e0" e2="DrugDDI.d474.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d474.s2" origId="s2" text="Since drowsiness may occur with use of this drug, patients should be warned of this possibility and cautioned against driving a car or operating dangerous machinery while taking Atarax."><entity id="DrugDDI.d474.s2.e0" origId="s2.p30" charOffset="44-48" type="drug" text="drug"/><entity id="DrugDDI.d474.s2.e1" origId="s2.p42" charOffset="178-184" type="drug" text="Atarax"/><pair id="DrugDDI.d474.s2.p0" e1="DrugDDI.d474.s2.e0" e2="DrugDDI.d474.s2.e1" interaction="false"/></sentence><sentence id="DrugDDI.d474.s3" origId="s3" text="Patients should be advised against the simultaneous use of other CNS depressant drugs, and cautioned that the effect of alcohol may be increased."><entity id="DrugDDI.d474.s3.e0" origId="s3.p48" charOffset="65-85" type="drug" text="CNS depressant drugs"/><entity id="DrugDDI.d474.s3.e1" origId="s3.p52" charOffset="120-127" type="drug" text="alcohol"/><pair id="DrugDDI.d474.s3.p0" e1="DrugDDI.d474.s3.e0" e2="DrugDDI.d474.s3.e1" interaction="true"/></sentence></document>