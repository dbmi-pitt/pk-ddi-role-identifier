<document id="DrugDDI.d24" origId="Ethambutol"><sentence id="DrugDDI.d24.s0" origId="s0" text="The results of a study of coadministration of ethambutol (50 mg/kg) with an aluminum hydroxide containing antacid to 13 patients with tuberculosis showed a reduction of mean serum concentrations and urinary excretion of ethambutol of approximately 20% and 13%, respectively, suggesting that the oral absorption of ethambutol may be reduced by these antacid products."><entity id="DrugDDI.d24.s0.e0" origId="s0.p3" charOffset="46-56" type="drug" text="ethambutol"/><entity id="DrugDDI.d24.s0.e1" origId="s0.p8" charOffset="76-114" type="drug" text="aluminum hydroxide containing antacid"/><entity id="DrugDDI.d24.s0.e2" origId="s0.p16" charOffset="220-230" type="drug" text="ethambutol"/><entity id="DrugDDI.d24.s0.e3" origId="s0.p23" charOffset="314-324" type="drug" text="ethambutol"/><entity id="DrugDDI.d24.s0.e4" origId="s0.p27" charOffset="349-356" type="drug" text="antacid"/><pair id="DrugDDI.d24.s0.p0" e1="DrugDDI.d24.s0.e0" e2="DrugDDI.d24.s0.e1" interaction="true"/><pair id="DrugDDI.d24.s0.p1" e1="DrugDDI.d24.s0.e0" e2="DrugDDI.d24.s0.e2" interaction="false"/><pair id="DrugDDI.d24.s0.p2" e1="DrugDDI.d24.s0.e0" e2="DrugDDI.d24.s0.e3" interaction="false"/><pair id="DrugDDI.d24.s0.p3" e1="DrugDDI.d24.s0.e0" e2="DrugDDI.d24.s0.e4" interaction="false"/><pair id="DrugDDI.d24.s0.p4" e1="DrugDDI.d24.s0.e1" e2="DrugDDI.d24.s0.e2" interaction="false"/><pair id="DrugDDI.d24.s0.p5" e1="DrugDDI.d24.s0.e1" e2="DrugDDI.d24.s0.e3" interaction="false"/><pair id="DrugDDI.d24.s0.p6" e1="DrugDDI.d24.s0.e1" e2="DrugDDI.d24.s0.e4" interaction="false"/><pair id="DrugDDI.d24.s0.p7" e1="DrugDDI.d24.s0.e2" e2="DrugDDI.d24.s0.e3" interaction="false"/><pair id="DrugDDI.d24.s0.p8" e1="DrugDDI.d24.s0.e2" e2="DrugDDI.d24.s0.e4" interaction="false"/><pair id="DrugDDI.d24.s0.p9" e1="DrugDDI.d24.s0.e3" e2="DrugDDI.d24.s0.e4" interaction="true"/></sentence><sentence id="DrugDDI.d24.s1" origId="s1" text="It is 