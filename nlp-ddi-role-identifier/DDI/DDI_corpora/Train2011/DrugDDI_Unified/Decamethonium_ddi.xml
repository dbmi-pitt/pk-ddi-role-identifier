<document id="DrugDDI.d277" origId="Decamethonium"><sentence id="DrugDDI.d277.s0" origId="s0" text="Potentially fatal drug interactions may occur when coadministered with digoxin, as this may enhance cardiovascular depression and bradyarrhythmias may occur."><entity id="DrugDDI.d277.s0.e0" origId="s0.p5" charOffset="71-78" type="drug" text="digoxin"/></sentence><sentence id="DrugDDI.d277.s1" origId="s1" text="Anticholinesterases (neostgmine, physostigmine), lignocaine, quinine, procainamide can enhance toxicity and cause cardio respiratory depression."><entity id="DrugDDI.d277.s1.e0" origId="s1.p15" charOffset="0-19" type="drug" text="Anticholinesterases"/><entity id="DrugDDI.d277.s1.e1" origId="s1.p18" charOffset="33-46" type="drug" text="physostigmine"/><entity id="DrugDDI.d277.s1.e2" origId="s1.p21" charOffset="49-59" type="drug" text="lignocaine"/><entity id="DrugDDI.d277.s1.e3" origId="s1.p22" charOffset="61-68" type="drug" text="quinine"/><entity id="DrugDDI.d277.s1.e4" origId="s1.p23" charOffset="70-82" type="drug" text="procainamide"/><pair id="DrugDDI.d277.s1.p0" e1="DrugDDI.d277.s1.e0" e2="DrugDDI.d277.s1.e1" interaction="false"/><pair id="DrugDDI.d277.s1.p1" e1="DrugDDI.d277.s1.e0" e2="DrugDDI.d277.s1.e2" interaction="false"/><pair id="DrugDDI.d277.s1.p2" e1="DrugDDI.d277.s1.e0" e2="DrugDDI.d277.s1.e3" interaction="false"/><pair id="DrugDDI.d277.s1.p3" e1="DrugDDI.d277.s1.e0" e2="DrugDDI.d277.s1.e4" interaction="false"/><pair id="DrugDDI.d277.s1.p4" e1="DrugDDI.d277.s1.e1" e2="DrugDDI.d277.s1.e2" interaction="false"/><pair id="DrugDDI.d277.s1.p5" e1="DrugDDI.d277.s1.e1" e2="DrugDDI.d277.s1.e3" interaction="false"/><pair id="DrugDDI.d277.s1.p6" e1="DrugDDI.d277.s1.e1" e2="DrugDDI.d277.s1.e4" interaction="false"/><pair id="DrugDDI.d277.s1.p7" e1="DrugDDI.d277.s1.e2" e2="DrugDDI.d277.s1.e3" interaction="false"/><pair id="DrugDDI.d277.s1.p8" e1="DrugDDI.d277.s1.e2" e2="DrugDDI.d277.s1.e4" interaction="false"/><pair id="DrugDDI.d277.s1.p9" e1="DrugDDI.d277.s1.