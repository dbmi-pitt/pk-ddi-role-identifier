<document id="DrugDDI.d111" origId="Nesiritide"><sentence id="DrugDDI.d111.s0" origId="s0" text="No trials specifically examining potential drug interactions with Natrecor were conducted, although many concomitant drugs were used in clinical trials."><entity id="DrugDDI.d111.s0.e0" origId="s0.p3" charOffset="66-74" type="drug" text="Natrecor"/><entity id="DrugDDI.d111.s0.e1" origId="s0.p8" charOffset="117-122" type="drug" text="drugs"/><pair id="DrugDDI.d111.s0.p0" e1="DrugDDI.d111.s0.e0" e2="DrugDDI.d111.s0.e1" interaction="false"/></sentence><sentence id="DrugDDI.d111.s1" origId="s1" text="No drug interactions were detected except for an increase in symptomatic hypotension in patients receiving oral ACE inhibitors."><entity id="DrugDDI.d111.s1.e0" origId="s1.p19" charOffset="112-126" type="drug" text="ACE inhibitors"/></sentence><sentence id="DrugDDI.d111.s2" origId="s2" text="The co-administration of Natrecor with IV vasodilators such as nitroglycerin, nitroprusside, milrinone, or IV ACE inhibitors has not been evaluated (these drugs were not co-administered with Natrecor in clinical trials)."><entity id="DrugDDI.d111.s2.e0" origId="s2.p21" charOffset="25-33" type="drug" text="Natrecor"/><entity id="DrugDDI.d111.s2.e1" origId="s2.p22" charOffset="42-54" type="drug" text="vasodilators"/><entity id="DrugDDI.d111.s2.e2" origId="s2.p23" charOffset="63-76" type="drug" text="nitroglycerin"/><entity id="DrugDDI.d111.s2.e3" origId="s2.p24" charOffset="78-91" type="drug" text="nitroprusside"/><entity id="DrugDDI.d111.s2.e4" origId="s2.p25" charOffset="93-102" type="drug" text="milrinone"/><entity id="DrugDDI.d111.s2.e5" origId="s2.p27" charOffset="110-124" type="drug" text="ACE inhibitors"/><entity id="DrugDDI.d111.s2.e6" origId="s2.p33" charOffset="155-160" type="drug" text="drugs"/><entity id="DrugDDI.d111.s2.e7" origId="s2.p37" charOffset="191-199" type="drug" text="Natrecor"/><pair id="DrugDDI.d111.s2.p0" e1="DrugDDI.d111.s2.e0" e2="DrugDDI.d111.s2.e1" interaction="fa