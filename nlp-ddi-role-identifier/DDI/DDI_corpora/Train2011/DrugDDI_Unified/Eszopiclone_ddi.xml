<pair id="DrugDDI.d273.s1.p2" e1="DrugDDI.d273.s1.e1" e2="DrugDDI.d273.s1.e2" interaction="false"/></sentence><sentence id="DrugDDI.d273.s2" origId="s2" text="Lorazepam Coadministration of single doses of eszopiclone 3 mg and lorazepam 2 mg did not have clinically relevant effects on the pharmacodynamics or pharmacokinetics of either drug."><entity id="DrugDDI.d273.s2.e0" origId="s2.p22" charOffset="0-9" type="drug" text="Lorazepam"/><entity id="DrugDDI.d273.s2.e1" origId="s2.p24" charOffset="46-62" type="drug" text="eszopiclone 3 mg"/><entity id="DrugDDI.d273.s2.e2" origId="s2.p26" charOffset="67-81" type="drug" text="lorazepam 2 mg"/><entity id="DrugDDI.d273.s2.e3" origId="s2.p36" charOffset="177-181" type="drug" text="drug"/><pair id="DrugDDI.d273.s2.p0" e1="DrugDDI.d273.s2.e0" e2="DrugDDI.d273.s2.e1" interaction="false"/><pair id="DrugDDI.d273.s2.p1" e1="DrugDDI.d273.s2.e0" e2="DrugDDI.d273.s2.e2" interaction="false"/><pair id="DrugDDI.d273.s2.p2" e1="DrugDDI.d273.s2.e0" e2="DrugDDI.d273.s2.e3" interaction="false"/><pair id="DrugDDI.d273.s2.p3" e1="DrugDDI.d273.s2.e1" e2="DrugDDI.d273.s2.e2" interaction="false"/><pair id="DrugDDI.d273.s2.p4" e1="DrugDDI.d273.s2.e1" e2="DrugDDI.d273.s2.e3" interaction="false"/><pair id="DrugDDI.d273.s2.p5" e1="DrugDDI.d273.s2.e2" e2="DrugDDI.d273.s2.e3" interaction="false"/></sentence><sentence id="DrugDDI.d273.s3" origId="s3" text="Olanzapine Coadministration of eszopiclone 3 mg and olanzapine 10 mg produced a decrease in DSST scores."><entity id="DrugDDI.d273.s3.e0" origId="s3.p37" charOffset="0-10" type="drug" text="Olanzapine"/><entity id="DrugDDI.d273.s3.e1" origId="s3.p38" charOffset="31-47" type="drug" text="eszopiclone 3 mg"/><entity id="DrugDDI.d273.s3.e2" origId="s3.p40" charOffset="52-68" type="drug" text="olanzapine 10 mg"/><pair id="DrugDDI.d273.s3.p0" e1="DrugDDI.d273.s3.e0" e2="DrugDDI.d273.s3.e1" interaction="false"/><pair id="DrugDDI.d273.s3.p1" e1="DrugDDI.d273.s3.e0" e2="DrugDDI.d273.s3.e2" interaction="false"/><pair id="DrugDDI.d273.s3.p2" e1="DrugDDI.d273.s3.e1" e2="DrugDDI.d273.s3.e2" interaction="true"/></sentence><sentence id="DrugDDI.d273.s4" origId="s4" text="The interaction was pharmacodynamic;"/><sentence id="DrugDDI.d273.s5" origId="s5" text="there was no alteration in the pharmacokinetics of either drug."><entity id="DrugDDI.d273.s5.e0" origId="s5.p54" charOffset="58-62" type="drug" text="drug"/></sentence><sentence id="DrugDDI.d273.s6" origId="s6" text="Drugs That Inhibit CYP3A4 (Ketoconazole) CYP3A4 is a major metabolic pathway for elimination of eszopiclone."><entity id="DrugDDI.d273.s6.e0" origId="s6.p55" charOffset="0-5" type="drug" text="Drugs"/><entity id="DrugDDI.d273.s6.e1" origId="s6.p58" charOffset="19-25" type="drug" text="CYP3A4"/><entity id="DrugDDI.d273.s6.e2" origId="s6.p60" charOffset="27-39" type="drug" text="Ketoconazole"/><entity id="DrugDDI.d273.s6.e3" origId="s6.p62" charOffset="41-47" type="drug" text="CYP3A4"/><entity id="DrugDDI.d273.s6.e4" origId="s6.p66" charOffset="96-107" type="drug" text="eszopiclone"/><pair id="DrugDDI.d273.s6.p0" e1="DrugDDI.d273.s6.e0" e2="DrugDDI.d273.s6.e1" interaction="false"/><pair id="DrugDDI.d273.s6.p1" e1="DrugDDI.d273.s6.e0" e2="DrugDDI.d273.s6.e2" interaction="false"/><pair id="DrugDDI.d273.s6.p2" e1="DrugDDI.d273.s6.e0" e2="DrugDDI.d273.s6.e3" interaction="false"/><pair id="DrugDDI.d273.s6.p3" e1="DrugDDI.d273.s6.e0" e2="DrugDDI.d273.s6.e4" interaction="true"/><pair id="DrugDDI.d273.s6.p4" e1="DrugDDI.d273.s6.e1" e2="DrugDDI.d273.s6.e2" interaction="false"/><pair id="DrugDDI.d273.s6.p5" e1="DrugDDI.d273.s6.e1" e2="DrugDDI.d273.s6.e3" interaction="false"/><pair id="DrugDDI.d273.s6.p6" e1="DrugDDI.d273.s6.e1" e2="DrugDDI.d273.s6.e4" interaction="false"/><pair id="DrugDDI.d273.s6.p7" e1="DrugDDI.d273.s6.e2" e2="DrugDDI.d273.s6.e3" interaction="false"/><pair id="DrugDDI.d273.s6.p8" e1="DrugDDI.d273.s6.e2" e2="DrugDDI.d273.s6.e4" interaction="false"/><pair id="DrugDDI.d273.s6.p9" e1="DrugDDI.d273.s6.e3" e2="DrugDDI.d273.s6.e4" interaction="false"/></sentence><sentence id="DrugDDI.d273.s7" origId="s7" text="The AUC of eszopiclone was increased 2.2-fold by coadministration of ketoconazole, a potent inhibitor of CYP3A4, 400 mg daily for 5 days."><entity id="DrugDDI.d273.s7.e0" origId="s7.p68" charOffset="11-22" type="drug" text="eszopiclone"/><entity id="DrugDDI.d273.s7.e1" origId="s7.p73" charOffset="69-81" type="drug" text="ketoconazole"/><entity id="DrugDDI.d273.s7.e2" origId="s7.p75" charOffset="105-111" type="drug" text="CYP3A4"/><pair id="DrugDDI.d273.s7.p0" e1="DrugDDI.d273.s7.e0" e2="DrugDDI.d273.s7.e1" interaction="true"/><pair id="DrugDDI.d273.s7.p1" e1="DrugDDI.d273.s7.e0" e2="DrugDDI.d273.s7.e2" interaction="false"/><pair id="DrugDDI.d273.s7.p2" e1="DrugDDI.d273.s7.e1" e2="DrugDDI.d273.s7.e2" interaction="false"/></sentence><sentence id="DrugDDI.d273.s8" origId="s8" text="Cmax and t1/2 were increased 1.4-fold and 1.3-fold, respectively."><entity id="DrugDDI.d273.s8.e0" origId="s8.p80" charOffset="9-11" type="drug" text="t1"/></sentence><sentence id="DrugDDI.d273.s9" origId="s9" text="Other strong inhibitors of CYP3A4 (e.g., itraconazole, clarithromycin, nefazodone, troleandomycin, ritonavir, nelfinavir) would be expected to behave similarly."><entity id="DrugDDI.d273.s9.e0" origId="s9.p89" charOffset="27-33" type="drug" text="CYP3A4"/><entity id="DrugDDI.d273.s9.e1" origId="s9.p92" charOffset="41-53" type="drug" text="itraconazole"/><entity id="DrugDDI.d273.s9.e2" origId="s9.p93" charOffset="55-69" type="drug" text="clarithromycin"/><entity id="DrugDDI.d273.s9.e3" origId="s9.p94" charOffset="71-81" type="drug" text="nefazodone"/><entity id="DrugDDI.d273.s9.e4" origId="s9.p95" charOffset="83-97" type="drug" text="troleandomycin"/><entity id="DrugDDI.d273.s9.e5" origId="s9.p96" charOffset="99-108" type="drug" text="ritonavir"/><entity id="DrugDDI.d273.s9.e6" origId="s9.p97" charOffset="110-120" type="drug" text="nelfinavir"/><pair id="DrugDDI.d273.s9.p0" e1="Dr