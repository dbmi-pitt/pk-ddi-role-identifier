<document id="DrugDDI.d434" origId="Conjugated Estrogens"><sentence id="DrugDDI.d434.s0" origId="s0" text="DRUG/LABORATORY TEST INTERACTIONS 1."><entity id="DrugDDI.d434.s0.e0" origId="s0.p0" charOffset="0-4" type="drug" text="DRUG"/></sentence><sentence id="DrugDDI.d434.s1" origId="s1" text="Accelerated prothrombin time, partial thromboplastin time, and platelet aggregation time;"/><sentence id="DrugDDI.d434.s2" origId="s2" text="increased platelet count;"/><sentence id="DrugDDI.d434.s3" origId="s3" text="increased factors II, VII antigen, VIII antigen, VIII coagulant activity, IX, X, XII, VII-X complex, II-VII-X complex, and beta-thromboglobulin;"><entity id="DrugDDI.d434.s3.e0" origId="s3.p9" charOffset="10-21" type="drug" text="factors II"/><entity id="DrugDDI.d434.s3.e1" origId="s3.p10" charOffset="26-33" type="drug" text="antigen"/><entity id="DrugDDI.d434.s3.e2" origId="s3.p11" charOffset="40-47" type="drug" text="antigen"/><entity id="DrugDDI.d434.s3.e3" origId="s3.p12" charOffset="49-64" type="drug" text="VIII coagulant"/><entity id="DrugDDI.d434.s3.e4" origId="s3.p19" charOffset="123-143" type="drug" text="beta-thromboglobulin"/><pair id="DrugDDI.d434.s3.p0" e1="DrugDDI.d434.s3.e0" e2="DrugDDI.d434.s3.e1" interaction="false"/><pair id="DrugDDI.d434.s3.p1" e1="DrugDDI.d434.s3.e0" e2="DrugDDI.d434.s3.e2" interaction="false"/><pair id="DrugDDI.d434.s3.p2" e1="DrugDDI.d434.s3.e0" e2="DrugDDI.d434.s3.e3" interaction="false"/><pair id="DrugDDI.d434.s3.p3" e1="DrugDDI.d434.s3.e0" e2="DrugDDI.d434.s3.e4" interaction="false"/><pair id="DrugDDI.d434.s3.p4" e1="DrugDDI.d434.s3.e1" e2="DrugDDI.d434.s3.e2" interaction="false"/><pair id="DrugDDI.d434.s3.p5" e1="DrugDDI.d434.s3.e1" e2="DrugDDI.d434.s3.e3" interaction="false"/><pair id="DrugDDI.d434.s3.p6" e1="DrugDDI.d434.s3.e1" e2="DrugDDI.d434.s3.e4" interaction="false"/><pair id="DrugDDI.d434.s3.p7" e1="DrugDDI.d434.s3.e2" e2="DrugDDI.d434.s3.e3" interaction="false"/><pair id="DrugDDI.d434.s3.p8" e1="DrugDDI.d434.s3.e2" e2="DrugDDI.d434.s3.e4" interaction="false"/><pair id="DrugDDI.d434.s3.p9" e1="DrugDDI.d434.s3.e3" e2="DrugDDI.d434.s3.e4" interaction="false"/></sentence><sentence id="DrugDDI.d434.s4" origId="s4" text="decreased levels of anti-factor Xa and antithrombin III, decreased antithrombin III activity;"><entity id="DrugDDI.d434.s4.e0" origId="s4.p22" charOffset="20-35" type="drug" text="anti-factor Xa"/><entity id="DrugDDI.d434.s4.e1" origId="s4.p24" charOffset="39-55" type="drug" text="antithrombin III"/><entity id="DrugDDI.d434.s4.e2" origId="s4.p25" charOffset="67-83" type="drug" text="antithrombin III"/><pair id="DrugDDI.d434.s4.p0" e1="DrugDDI.d434.s4.e0" e2="DrugDDI.d434.s4.e1" interaction="false"/><pair id="DrugDDI.d434.s4.p1" e1="DrugDDI.d434.s4.e0" e2="DrugDDI.d434.s4.e2" interaction="false"/><pair id="DrugDDI.d434.s4.p2" e1="DrugDDI.d434.s4.e1" e2="DrugDDI.d434.s4.e2" interaction="false"/></sentence><sentence id="DrugDDI.d434.s5" origId="s5" text="increased levels of fibrinogen and fibrinogen activity;"><entity id="DrugDDI.d434.s5.e0" origId="s5.p28" charOffset="20-30" type="drug" text="fibrinogen"/></sentence><sentence id="DrugDDI.d434.s6" origId="s6" text="increased plasminogen antigen and activity."/><sentence id="DrugDDI.d434.s7" origId="s7" text="2."/><sentence id="DrugDDI.d434.s8" origId="s8" text="Increased thyroid-binding globulin (TBG) levels leading to increased circulating total thyroid hormone levels as measured by protein-bound iodine (PBI), T4 levels (by column or by radioimmunoassay) or T3 levels by radioimmunoassay."><entity id="DrugDDI.d434.s8.e0" origId="s8.p44" charOffset="125-145" type="drug" text="protein-bound iodine"/></sentence><sentence id="DrugDDI.d434.s9" origId="s9" text="T3 resin uptake is decreased, reflecting the elevated TBG."/><sentence id="DrugDDI.d434.s10" origId="s10" text="Free T4 and free T3 concentrations are unaltered."/><sentence id="DrugDDI.d434.s11" origId="s11" text="Patients on thyroid replacement therapy may require higher doses of thyroid hormone."><entity id="DrugDDI.d434.s11.e0" origId="s11.p70" charOffset="12-19" type="drug" text="thyroid"/><entity id="DrugDDI.d434.s11.e1" origId="s11.p74" charOffset="68-83" type="drug" text="thyroid hormone"/><pair id="DrugDDI.d434.s11.p0" e1="DrugDDI.d434.s11.e0" e2="DrugDDI.d434.s11.e1" interaction="false"/></sentence><sentence id="DrugDDI.d434.s12" origId="s12" text="3."/><sentence id="DrugDDI.d434.s13" origId="s13" text="Other binding proteins may be elevated in serum, (i.e., corticosteroid binding globulin (CBG), sex hormone binding globulin (SHBG)) leading to increased total circulating corticosteroids and sex steroids, respectively."><entity id="DrugDDI.d434.s13.e0" origId="s13.p76" charOffset="6-22" type="drug" text="binding proteins"/><entity id="DrugDDI.d434.s13.e1" origId="s13.p83" charOffset="56-88" type="drug" text="corticosteroid binding globulin"/><entity id="DrugDDI.d434.s13.e2" origId="s13.p85" charOffset="89-92" type="drug" text="CBG"/><entity id="DrugDDI.d434.s13.e3" origId="s13.p88" charOffset="107-124" type="drug" text="binding globulin"/><entity id="DrugDDI.d434.s13.e4" origId="s13.p94" charOffset="171-186" type="drug" text="corticosteroids"/><pair id="DrugDDI.d434.s13.p0" e1="DrugDDI.d434.s13.e0" e2="DrugDDI.d434.s13.e1" interaction="false"/><pair id="DrugDDI.d434.s13.p1" e1="DrugDDI.d434.s13.e0" e2="DrugDDI.d434.s13.e2" interaction="false"/><pair id="DrugDDI.d434.s13.p2" e1="DrugDDI.d434.s13.e0" e2="DrugDDI.d434.s13.e3" interaction="false"/><pair id="DrugDDI.d434.s13.p3" e1="DrugDDI.d434.s13.e0" e2="DrugDDI.d434.s13.e4" interaction="false"/><pair id="DrugDDI.d434.s13.p4" e1="DrugDDI.d434.s13.e1" e2="DrugDDI.d434.s13.e2" interaction="false"/><pair id="DrugDDI.d434.s13.p5" e1="DrugDDI.d434.s13.e1" e2="DrugDDI.d434.s13.e3" interaction="false"/><pair id="DrugDDI.d434.s13.p6" e1="DrugDDI.d434.s13.e1" e2="DrugDDI.d434.s13.e4" interaction="false"/><pair id="DrugDDI.d434.s13.p7" e1="DrugDDI.d434.s13.e2" e2="DrugDDI.d434.s13.e3" interaction="false"/><pair id="DrugDDI.d434.s13.p8" e1="DrugDDI.d434.s13.e2" e2="DrugDDI.d434.s13.e4" interaction="false"/><pair id="DrugDDI.d434.s13.p9" e1="DrugDDI.d434.s13.e3" e2="DrugDDI.d434.s13.e4" interaction="false"/></sentence><sentence id="DrugDDI.d434.s14" origId="s14" text="Free hormone concentrations may be decreased."/><sentence id="DrugDDI.d434.s15" origId="s15" text="Other plasma proteins may be increased (angiotensinogen/renin substrate, alpha-1-antitrypsin, ceruloplasmin)."><entity id="DrugDDI.d434.s15.e0" origId="s15.p102" charOffset="6-21" type="drug" text="plasma proteins"/><entity id="DrugDDI.d434.s15.e1" origId="s15.p107" charOffset="40-55" type="drug" text="angiotensinogen"/><entity id="DrugDDI.d434.s15.e2" origId="s15.p108" charOffset="55-72" type="drug" text="/renin substrate"/><entity id="DrugDDI.d434.s15.e3" origId="s15.p109" charOffset="73-93" type="drug" text="alpha-1-antitrypsin"/><entity id="DrugDDI.d434.s15.e4" origId="s15.p110" charOffset="94-107" type="drug" text="ceruloplasmin"/><pair id="DrugDDI.d434.s15.p0" e1="DrugDDI.d434.s15.e0" e2="DrugDDI.d434.s15.e1" interaction="false"/><pair id="DrugDDI.d434.s15.p1" e1="DrugDDI.d434.s15.e0" e2="DrugDDI.d434.s15.e2" interaction="false"/><pair id="DrugDDI.d434.s15.p2" e1="DrugDDI.d434.s15.e0" e2="DrugDDI.d434.s15.e3" interaction="false"/><pair id="DrugDDI.d434.s15.p3" e1="DrugDDI.d434.s15.e0" e2="DrugDDI.d434.s15.e4" interaction="false"/><pair id="DrugDDI.d434.s15.p4" e1="DrugDDI.d434.s15.e1" e2="DrugDDI.d434.s15.e2" interaction="false"/><pair id="DrugDDI.d434.s15.p5" e1="DrugDDI.d434.s15.e1" e2="DrugDDI.d434.s15.e3" interaction="false"/><pair id="DrugDDI.d434.s15.p6" e1="DrugDDI.d434.s15.e1" e2="DrugDDI.d434.s15.e4" interaction="false"/><pair id="DrugDDI.d434.s15.p7" e1="DrugDDI.d434.s15.e2" e2="DrugDDI.d434.s15.e3" interaction="false"/><pair id="DrugDDI.d434.s15.p8" e1="DrugDDI.d434.s15.e2" e2="DrugDDI.d434.s15.e4" interaction="false"/><pair id="DrugDDI.d434.s15.p9" e1="DrugDDI.d