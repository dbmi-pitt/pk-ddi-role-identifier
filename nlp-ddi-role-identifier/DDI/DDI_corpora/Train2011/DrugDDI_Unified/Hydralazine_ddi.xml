<document id="DrugDDI.d46" origId="Hydralazine"><sentence id="DrugDDI.d46.s0" origId="s0" text="MAO inhibitors should be used with caution in patients receiving hydralazine."><entity id="DrugDDI.d46.s0.e0" origId="s0.p0" charOffset="0-14" type="drug" text="MAO inhibitors"/><entity id="DrugDDI.d46.s0.e1" origId="s0.p7" charOffset="65-76" type="drug" text="hydralazine"/><pair id="DrugDDI.d46.s0.p0" e1="DrugDDI.d46.s0.e0" e2="DrugDDI.d46.s0.e1" interaction="true"/></sentence><sentence id="DrugDDI.d46.s1" origId="s1" text="When other potent parental antihypertensive drugs, such as diazoxide, are used in combination with hydralazine, patients should be continuously observed for several hours for any excessive fall in blood pressure."><entity id="DrugDDI.d46.s1.e0" origId="s1.p9" charOffset="27-49" type="drug" text="antihypertensive drugs"/><entity id="DrugDDI.d46.s1.e1" origId="s1.p10" charOffset="59-68" type="drug" text="diazoxide"/><entity id="DrugDDI.d46.s1.e2" origId="s1.p14" charOffset="99-110" type="drug" text="hydralazine"/><pair id="DrugDDI.d46.s1.p0" e1="DrugDDI.d46.s1.e0" e2="DrugDDI.d46.s1.e1" interaction="false"/><pair id="DrugDDI.d46.s1.p1" e1="DrugDDI.d46.s1.e0" e2="DrugDDI.d46.s1.e2" interaction="true"/><pair id="DrugDDI.d46.s1.p2" e1="DrugDDI.d46.s1.e1" e2="DrugDDI.d46.s1.e2" interaction="false"/></sentence><sentence id="DrugDDI.d46.s2" origId="s2" text="Profound hypotensive episodes may occur when diazoxide infection and hydralazine are used concomitantly."><entity id="DrugDDI.d46.s2.e0" origId="s2.p27" charOffset="45-54" type="drug" text="diazoxide"/><entity id="DrugDDI.d46.s2.e1" origId="s2.p29" charOffset="69-80" type="drug" text="hydralazine"/><pair id="DrugDDI.d46.s2.p0" e1="DrugDDI.d46.s2.e0" e2="DrugDDI.d46.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d46.s3" origId="s3" text="Beta-blockers (metoprolol, propranolol) serum concentrations and pharmacologic effects may be increased."><entity id="DrugDDI.d46.s3.e0" origId="s3.p33" charOffset="0-14" type="drug" text="Beta-blockers"/><entity id="DrugDDI.d46.s3.e1" origId="s3.p35" charOffset="15-25" type="drug" text="metoprolol"/><entity id="DrugDDI.d46.s3.e2" origId="s3.p36" charOffset="27-38" type="drug" text="propranolol"/><pair id="DrugDDI.d46.s3.p0" e1="DrugDDI.d46.s3.e0" e2="DrugDDI.d46.s3.e1" interaction="false"/><pair id="DrugDDI.d46.s3.p1" e1="DrugDDI.d46.s3.e0" e2="DrugDDI.d46.s3.e2" interaction="false"/><pair id="DrugDDI.d46.s3.p2" e1="DrugDDI.d46.s3.e1" e2="DrugDDI.d46.s3.e2" interaction="false"/></sentence><sentence id="DrugDDI.d46.s4" origId="s4" text="Monitor cardiovascular status."/><sentence id="DrugDDI.d46.s5" origId="s5" text="Propranolol increases hydralazines serum concentrations."><entity id="DrugDDI.d46.s5.e0" origId="s5.p46" charOffset="0-11" type="drug" text="Propranolol"/><entity id="DrugDDI.d46.s5.e1" origId="s5.p48" charOffset="22-33" type="drug" text="hydralazine"/><pair id="DrugDDI.d46.s5.p0" e1="DrugDDI.d46.s5.e0" e2="DrugDDI.d46.s5.e1" interaction="true"/></sentence><sentence id="DrugDDI.d46.s6" origId="s6" text="Acebutolol, atenolol, and nadolol (low hepatic clearance or no first-pass metabolism) are unlikely to be affected."><entity id="DrugDDI.d46.s6.e0" origId="s6.p51" charOffset="0-10" type="drug" text="Acebutolol"/><entity id="DrugDDI.d46.s6.e1" origId="s6.p52" charOffset="12-20" type="drug" text="atenolol"/><entity id="DrugDDI.d46.s6.e2" origId="s6.p54" charOffset="26-33" type="drug" text="nadolol"/><pair id="DrugDDI.d46.s6.p0" e1="DrugDDI.d46.s6.e0" e2="DrugDDI.d46.s6.e1" interaction="false"/><pair id="DrugDDI.d46.s6.p1" e1="DrugDDI.d46.s6.e0" e2="DrugDDI.d46.s6.e2" interaction="false"/><pair id="DrugDDI.d46.s6.p2" e1="DrugDDI.d46.s6.e1" e2="DrugDDI.d46.s6.e2" interaction="false"/></sentence><sentence id="DrugDDI.d46.s7" origId="s7" text="NSAIDs may decrease the hemodynamic effects of hydralazine;"><entity id="DrugDDI.d46.s7.e0" origId="s7.p66" charOffset="0-6" type="drug" text="NSAIDs"/><entity id="DrugDDI.d46.s7.e1" origId="s7.p70" charOffset="47-58" type="drug" text="hydralazine"/><pair id="DrugDDI.d46.s7.p0" e1="DrugDDI.d46.s7.e0" e2="DrugDDI.d46.s7.e1" interaction="true"/></sentence><sentence id="DrugDDI.d46.s8" origId="s8" text="avoid use if possible or closely monitor cardiovascular status at the end of drug interactions"/></document>