<document id="DrugDDI.d546" origId="Diethylpropion"><sentence id="DrugDDI.d546.s0" origId="s0" text="Antidiabetic drug requirements (i.e., insulin) may be altered."><entity id="DrugDDI.d546.s0.e0" origId="s0.p0" charOffset="0-17" type="drug" text="antidiabetic drug"/><entity id="DrugDDI.d546.s0.e1" origId="s0.p3" charOffset="38-45" type="drug" text="insulin"/><pair id="DrugDDI.d546.s0.p0" e1="DrugDDI.d546.s0.e0" e2="DrugDDI.d546.s0.e1" interaction="false"/></sentence><sentence id="DrugDDI.d546.s1" origId="s1" text="Concurrent use with general anesthetics may result in arrhythmias."><entity id="DrugDDI.d546.s1.e0" origId="s1.p9" charOffset="20-39" type="drug" text="general anesthetics"/></sentence><sentence id="DrugDDI.d546.s2" origId="s2" text="The pressor effects of diethylpropion and those of other drugs may be additive when the drugs are used concomitantly;"><entity id="DrugDDI.d546.s2.e0" origId="s2.p13" charOffset="4-19" type="drug" text="pressor effects"/><entity id="DrugDDI.d546.s2.e1" origId="s2.p14" charOffset="23-37" type="drug" text="diethylpropion"/><entity id="DrugDDI.d546.s2.e2" origId="s2.p16" charOffset="57-62" type="drug" text="drugs"/><entity id="DrugDDI.d546.s2.e3" origId="s2.p21" charOffset="88-93" type="drug" text="drugs"/><pair id="DrugDDI.d546.s2.p0" e1="DrugDDI.d546.s2.e0" e2="DrugDDI.d546.s2.e1" interaction="false"/><pair id="DrugDDI.d546.s2.p1" e1="DrugDDI.d546.s2.e0" e2="DrugDDI.d546.s2.e2" interaction="false"/><pair id="DrugDDI.d546.s2.p2" e1="DrugDDI.d546.s2.e0" e2="DrugDDI.d546.s2.e3" interaction="false"/><pair id="DrugDDI.d546.s2.p3" e1="DrugDDI.d546.s2.e1" e2="DrugDDI.d546.s2.e2" interaction="true"/><pair id="DrugDDI.d546.s2.p4" e1="DrugDDI.d546.s2.e1" e2="DrugDDI.d546.s2.e3" interaction="false"/><pair id="DrugDDI.d546.s2.p5" e1="DrugDDI.d546.s2.e2" e2="DrugDDI.d546.s2.e3" interaction="false"/></sentence><sentence id="DrugDDI.d546.s3" origId="s3" text="conversely, diethylpropion may interfere with antihypertensive drugs (i.e., guanethidine, a-methyldopa)."><entity id="DrugDDI.d546.s3.e0" origId="s3.p27" charOffset="12-26" type="drug" text="diethylpropion"/><entity id="DrugDDI.d546.s3.e1" origId="s3.p30" charOffset="46-68" type="drug" text="antihypertensive drugs"/><entity id="DrugDDI.d546.s3.e2" origId="s3.p33" charOffset="76-88" type="drug" text="guanethidine"/><entity id="DrugDDI.d546.s3.e3" origId="s3.p34" charOffset="92-102" type="drug" text="methyldopa"/><pair id="DrugDDI.d546.s3.p0" e1="DrugDDI.d546.s3.e0" e2="DrugDDI.d546.s3.e1" interaction="false"/><pair id="DrugDDI.d546.s3.p1" e1="DrugDDI.d546.s3.e0" e2="DrugDDI.d546.s3.e2" interaction="true"/><pair id="DrugDDI.d546.s3.p2" e1="DrugDDI.d546.s3.e0" e2="DrugDDI.d546.s3.e3" interaction="true"/><pair id="DrugDDI.d546.s3.p3" e1="DrugDDI.d546.s3.e1" e2="DrugDDI.d546.s3.e2" interaction="false"/><pair id="DrugDDI.d546.s3.p4" e1="DrugDDI.d546.s3.e1" e2="DrugDDI.d546.s3.e3" interaction="false"/><pair id="DrugDDI.d546.s3.p5" e1="DrugDDI.d546.s3.e2" e2="DrugDDI.d546.s3.e3" interaction="false"/></sentence><sentence id="DrugDDI.d546.s4" origId="s4" text="Concurrent use of phenothiazines may antagonize the anorectic effect of diethylpropion."><entity id="DrugDDI.d546.s4.e0" origId="s4.p37" charOffset="18-32" type="drug" text="phenothiazines"/><entity id="DrugDDI.d546.s4.e1" origId="s4.p41" charOffset="72-86" type="drug" text="diethylpropion"/><pair id="DrugDDI.d546.s4.p0" e1="DrugDDI.d546.s4.e0" e2="DrugDDI.d546.s4.e1" interaction="true"/></sentence></document>