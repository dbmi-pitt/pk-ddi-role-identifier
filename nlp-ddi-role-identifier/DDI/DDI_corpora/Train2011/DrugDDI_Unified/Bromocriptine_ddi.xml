<document id="DrugDDI.d411" origId="Bromocriptine"><sentence id="DrugDDI.d411.s0" origId="s0" text="The risk of using bromocriptine mesylate in combination with other drugs has not been systematically evaluated, but alcohol may potentiate the side effects of bromocriptine mesylate."><entity id="DrugDDI.d411.s0.e0" origId="s0.p3" charOffset="18-40" type="drug" text="bromocriptine mesylate"/><entity id="DrugDDI.d411.s0.e1" origId="s0.p5" charOffset="67-72" type="drug" text="drugs"/><entity id="DrugDDI.d411.s0.e2" origId="s0.p13" charOffset="116-123" type="drug" text="alcohol"/><entity id="DrugDDI.d411.s0.e3" origId="s0.p17" charOffset="159-181" type="drug" text="bromocriptine mesylate"/><pair id="DrugDDI.d411.s0.p0" e1="DrugDDI.d411.s0.e0" e2="DrugDDI.d411.s0.e1" interaction="false"/><pair id="DrugDDI.d411.s0.p1" e1="DrugDDI.d411.s0.e0" e2="DrugDDI.d411.s0.e2" interaction="false"/><pair id="DrugDDI.d411.s0.p2" e1="DrugDDI.d411.s0.e0" e2="DrugDDI.d411.s0.e3" interaction="false"/><pair id="DrugDDI.d411.s0.p3" e1="DrugDDI.d411.s0.e1" e2="DrugDDI.d411.s0.e2" interaction="false"/><pair id="DrugDDI.d411.s0.p4" e1="DrugDDI.d411.s0.e1" e2="DrugDDI.d411.s0.e3" interaction="false"/><pair id="DrugDDI.d411.s0.p5" e1="DrugDDI.d411.s0.e2" e2="DrugDDI.d411.s0.e3" interaction="true"/></sentence><sentence id="DrugDDI.d411.s1" origId="s1" text="Bromocriptine mesylate may interact with dopamine antagonists, butyrophenones, and certain other agents."><entity id="DrugDDI.d411.s1.e0" origId="s1.p18" charOffset="0-22" type="drug" text="Bromocriptine mesylate"/><entity id="DrugDDI.d411.s1.e1" origId="s1.p21" charOffset="41-61" type="drug" text="dopamine antagonists"/><entity id="DrugDDI.d411.s1.e2" origId="s1.p22" charOffset="63-77" type="drug" text="butyrophenones"/><pair id="DrugDDI.d411.s1.p0" e1="DrugDDI.d411.s1.e0" e2="DrugDDI.d411.s1.e1" interaction="true"/><pair id="DrugDDI.d411.s1.p1" e1="DrugDDI.d411.s1.e0" e2="DrugDDI.d411.s1.e2" interaction="true"/><pair id="DrugDDI.d411.s1.p2" e1="DrugDDI.d411.s1.e1" e2="DrugDDI.d411.s1.e2" interaction="false"/></sentence><sentence id="DrugDDI.d411.s2" origId="s2" text="Compounds in these categories result in a decreased efficacy of bromocriptine mesylate: phenothiazines, haloperidol, metoclopramide, pimozide."><entity id="DrugDDI.d411.s2.e0" origId="s2.p29" charOffset="64-86" type="drug" text="bromocriptine mesylate"/><entity id="DrugDDI.d411.s2.e1" origId="s2.p31" charOffset="88-102" type="drug" text="phenothiazines"/><entity id="DrugDDI.d411.s2.e2" origId="s2.p32" charOffset="104-115" type="drug" text="haloperidol"/><entity id="DrugDDI.d411.s2.e3" origId="s2.p33" charOffset="117-131" type="drug" text="metoclopramide"/><entity id="DrugDDI.d411.s2.e4" origId="s2.p34" charOffset="133-141" type="drug" text="pimozide"/><pair id="DrugDDI.d411.s2.p0" e1="DrugDDI.d411.s2.e0" e2="DrugDDI.d411.s2.e1" interaction="true"/><pair id="DrugDDI.d411.s2.p1" e1="DrugDDI.d411.s2.e0" e2="DrugDDI.d411.s2.e2" interaction="true"/><pair id="DrugDDI.d411.s2.p2" e1="DrugDDI.d411.s2.e0" e2="DrugDDI.d411.s2.e3" interaction="true"/><pair id="DrugDDI.d411.s2.p3" e1="DrugDDI.d411.s2.e0" e2="DrugDDI.d411.s2.e4" interaction="true"/><pair id="DrugDDI.d411.s2.p4" e1="DrugDDI.d411.s2.e1" e2="DrugDDI.d411.s2.e2" interaction="false"/><pair id="DrugDDI.d411.s2.p5" e1="DrugDDI.d411.s2.e1" e2="DrugDDI.d411.s2.e3" interaction="false"/><pair id="DrugDDI.d411.s2.p6" e1="DrugDDI.d411.s2.e1" e2="DrugDDI.d411.s2.e4" interaction="false"/><pair id="DrugDDI.d411.s2.p7" e1="DrugDDI.d411.s2.e2" e2="DrugDDI.d411.s2.e3" interaction="false"/><pair id="DrugDDI.d411.s2.p8" e1="DrugDDI.d411.s2.e2" e2="DrugDDI.d411.s2.e4" interaction="false"/><pair id="DrugDDI.d411.s2.p9" e1="DrugDDI.d411.s2.e3" e2="DrugDDI.d411.s2.e4" interaction="false"/></sentence><sentence id="DrugDDI.d411.s3" origId="s3" text="Concomitant use of bromocriptine mesylate with other ergot alkaloids is not recommended."><entity id="DrugDDI.d411.s3.e0" origId="s3.p36" charOffset="19-41" type="drug