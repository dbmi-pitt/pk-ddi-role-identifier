<document id="DrugDDI.d209" origId="L-Arginine"><sentence id="DrugDDI.d209.s0" origId="s0" text="Cyclosporine - L-arginine may counteract the antinaturetic effect of cyclosporin."><entity id="DrugDDI.d209.s0.e0" origId="s0.p0" charOffset="0-12" type="drug" text="Cyclosporine"/><entity id="DrugDDI.d209.s0.e1" origId="s0.p4" charOffset="69-81" type="drug" text="cyclosporin"/><pair id="DrugDDI.d209.s0.p0" e1="DrugDDI.d209.s0.e0" e2="DrugDDI.d209.s0.e1" interaction="true"/></sentence><sentence id="DrugDDI.d209.s1" origId="s1" text="Ibuprofen - L-arginine may increase the absorption of ibuprofen if taken concomitantly."><entity id="DrugDDI.d209.s1.e0" origId="s1.p5" charOffset="0-23" type="drug" text="Ibuprofen - L-arginine"/><entity id="DrugDDI.d209.s1.e1" origId="s1.p9" charOffset="54-63" type="drug" text="ibuprofen"/><pair id="DrugDDI.d209.s1.p0" e1="DrugDDI.d209.s1.e0" e2="DrugDDI.d209.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d209.s2" origId="s2" text="Organic nitrates - L-arginine supplements theoretically may potentiate the effects of organic nitrates if taken concomitantly."><entity id="DrugDDI.d209.s2.e0" origId="s2.p13" charOffset="8-16" type="drug" text="nitrates"/><entity id="DrugDDI.d209.s2.e1" origId="s2.p19" charOffset="94-102" type="drug" text="nitrates"/><pair id="DrugDDI.d209.s2.p0" e1="DrugDDI.d209.s2.e0" e2="DrugDDI.d209.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d209.s3" origId="s3" text="Sildenafil citrate - Theoretically, L-arginine supplements taken concomitantly with sildenafil citrate, may potentiate the effects of the drug."><entity id="DrugDDI.d209.s3.e0" origId="s3.p23" charOffset="0-18" type="drug" text="Sildenafil citrate"/><entity id="DrugDDI.d209.s3.e1" origId="s3.p24" charOffset="38-46" type="drug" text="arginine"/><entity id="DrugDDI.d209.s3.e2" origId="s3.p27" charOffset="84-102" type="drug" text="sildenafil citrate"/><entity id="DrugDDI.d209.s3.e3" origId="s3.p31" charOffset="138-142" type="drug" tex