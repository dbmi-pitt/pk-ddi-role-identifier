<document id="DrugDDI.d299" origId="Clemastine"><sentence id="DrugDDI.d299.s0" origId="s0" text="Additive CNS depression may occur when antihistamines are administered concomitantly with other CNS depressants including barbiturates, tranquilizers, and alcohol."><entity id="DrugDDI.d299.s0.e0" origId="s0.p4" charOffset="39-53" type="drug" text="antihistamines"/><entity id="DrugDDI.d299.s0.e1" origId="s0.p8" charOffset="96-111" type="drug" text="CNS depressants"/><entity id="DrugDDI.d299.s0.e2" origId="s0.p10" charOffset="122-134" type="drug" text="barbiturates"/><entity id="DrugDDI.d299.s0.e3" origId="s0.p11" charOffset="136-149" type="drug" text="tranquilizers"/><entity id="DrugDDI.d299.s0.e4" origId="s0.p13" charOffset="155-162" type="drug" text="alcohol"/><pair id="DrugDDI.d299.s0.p0" e1="DrugDDI.d299.s0.e0" e2="DrugDDI.d299.s0.e1" interaction="false"/><pair id="DrugDDI.d299.s0.p1" e1="DrugDDI.d299.s0.e0" e2="DrugDDI.d299.s0.e2" interaction="false"/><pair id="DrugDDI.d299.s0.p2" e1="DrugDDI.d299.s0.e0" e2="DrugDDI.d299.s0.e3" interaction="false"/><pair id="DrugDDI.d299.s0.p3" e1="DrugDDI.d299.s0.e0" e2="DrugDDI.d299.s0.e4" interaction="false"/><pair id="DrugDDI.d299.s0.p4" e1="DrugDDI.d299.s0.e1" e2="DrugDDI.d299.s0.e2" interaction="false"/><pair id="DrugDDI.d299.s0.p5" e1="DrugDDI.d299.s0.e1" e2="DrugDDI.d299.s0.e3" interaction="false"/><pair id="DrugDDI.d299.s0.p6" e1="DrugDDI.d299.s0.e1" e2="DrugDDI.d299.s0.e4" interaction="false"/><pair id="DrugDDI.d299.s0.p7" e1="DrugDDI.d299.s0.e2" e2="DrugDDI.d299.s0.e3" interaction="false"/><pair id="DrugDDI.d299.s0.p8" e1="DrugDDI.d299.s0.e2" e2="DrugDDI.d299.s0.e4" interaction="false"/><pair id="DrugDDI.d299.s0.p9" e1="DrugDDI.d299.s0.e3" e2="DrugDDI.d299.s0.e4" interaction="false"/></sentence><sentence id="DrugDDI.d299.s1" origId="s1" text="Patients receiving antihistamines should be advised against the concurrent use of other CNS depressant drugs."><entity id="DrugDDI.d299.s1.e0" origId="s1.p16" charOffset="19-33" type