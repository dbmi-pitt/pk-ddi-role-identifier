<document id="DrugDDI.d318" origId="Alendronate"><sentence id="DrugDDI.d318.s0" origId="s0" text="Intravenous ranitidine was shown to double the bioavailability of oral alendronate."><entity id="DrugDDI.d318.s0.e0" origId="s0.p0" charOffset="12-22" type="drug" text="ranitidine"/><entity id="DrugDDI.d318.s0.e1" origId="s0.p6" charOffset="71-82" type="drug" text="alendronate"/><pair id="DrugDDI.d318.s0.p0" e1="DrugDDI.d318.s0.e0" e2="DrugDDI.d318.s0.e1" interaction="true"/></sentence><sentence id="DrugDDI.d318.s1" origId="s1" text="The clinical significance of this increased bioavailability and whether similar increases will occur in patients given oral H2-antagonists is unknown;"/><sentence id="DrugDDI.d318.s2" origId="s2" text="no other specific drug interaction studies were performed."/><sentence id="DrugDDI.d318.s3" origId="s3" text="Products containing calcium and other multivalent cations likely will interfere with absorption of alendronate."><entity id="DrugDDI.d318.s3.e0" origId="s3.p24" charOffset="20-27" type="drug" text="calcium"/><entity id="DrugDDI.d318.s3.e1" origId="s3.p26" charOffset="50-57" type="drug" text="cations"/><entity id="DrugDDI.d318.s3.e2" origId="s3.p30" charOffset="99-110" type="drug" text="alendronate"/><pair id="DrugDDI.d318.s3.p0" e1="DrugDDI.d318.s3.e0" e2="DrugDDI.d318.s3.e1" interaction="false"/><pair id="DrugDDI.d318.s3.p1" e1="DrugDDI.d318.s3.e0" e2="DrugDDI.d318.s3.e2" interaction="true"/><pair id="DrugDDI.d318.s3.p2" e1="DrugDDI.d318.s3.e1" e2="DrugDDI.d318.s3.e2" interaction="true"/></sentence></document>