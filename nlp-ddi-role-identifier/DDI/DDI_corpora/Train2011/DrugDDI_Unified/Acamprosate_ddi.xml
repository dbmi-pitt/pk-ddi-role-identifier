<document id="DrugDDI.d426" origId="Acamprosate"><sentence id="DrugDDI.d426.s0" origId="s0" text="The concomitant intake of alcohol and Acamprosate does not affect the pharmacokinetics of either alcohol or acamprosate."><entity id="DrugDDI.d426.s0.e0" origId="s0.p1" charOffset="26-33" type="drug" text="alcohol"/><entity id="DrugDDI.d426.s0.e1" origId="s0.p3" charOffset="38-49" type="drug" text="Acamprosate"/><entity id="DrugDDI.d426.s0.e2" origId="s0.p10" charOffset="97-104" type="drug" text="alcohol"/><entity id="DrugDDI.d426.s0.e3" origId="s0.p12" charOffset="108-119" type="drug" text="acamprosate"/><pair id="DrugDDI.d426.s0.p0" e1="DrugDDI.d426.s0.e0" e2="DrugDDI.d426.s0.e1" interaction="false"/><pair id="DrugDDI.d426.s0.p1" e1="DrugDDI.d426.s0.e0" e2="DrugDDI.d426.s0.e2" interaction="false"/><pair id="DrugDDI.d426.s0.p2" e1="DrugDDI.d426.s0.e0" e2="DrugDDI.d426.s0.e3" interaction="false"/><pair id="DrugDDI.d426.s0.p3" e1="DrugDDI.d426.s0.e1" e2="DrugDDI.d426.s0.e2" interaction="false"/><pair id="DrugDDI.d426.s0.p4" e1="DrugDDI.d426.s0.e1" e2="DrugDDI.d426.s0.e3" interaction="false"/><pair id="DrugDDI.d426.s0.p5" e1="DrugDDI.d426.s0.e2" e2="DrugDDI.d426.s0.e3" interaction="false"/></sentence><sentence id="DrugDDI.d426.s1" origId="s1" text="Pharmacokinetic studies indicate that administration of disulfiram or diazepam does not affect the pharmacokinetics of acamprosate."><entity id="DrugDDI.d426.s1.e0" origId="s1.p16" charOffset="56-66" type="drug" text="disulfiram"/><entity id="DrugDDI.d426.s1.e1" origId="s1.p18" charOffset="70-78" type="drug" text="diazepam"/><entity id="DrugDDI.d426.s1.e2" origId="s1.p23" charOffset="119-130" type="drug" text="acamprosate"/><pair id="DrugDDI.d426.s1.p0" e1="DrugDDI.d426.s1.e0" e2="DrugDDI.d426.s1.e1" interaction="false"/><pair id="DrugDDI.d426.s1.p1" e1="DrugDDI.d426.s1.e0" e2="DrugDDI.d426.s1.e2" interaction="false"/><pair id="DrugDDI.d426.s1.p2" e1="DrugDDI.d426.s1.e1" e2="DrugDDI.d426.s1.e2" interaction="false"/></sentence><