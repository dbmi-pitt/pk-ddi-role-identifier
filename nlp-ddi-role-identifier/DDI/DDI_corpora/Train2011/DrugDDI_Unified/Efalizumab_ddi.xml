<document id="DrugDDI.d523" origId="Efalizumab"><sentence id="DrugDDI.d523.s0" origId="s0" text="No formal drug interaction studies have been performed with RAPTIVA."><entity id="DrugDDI.d523.s0.e0" origId="s0.p4" charOffset="60-67" type="drug" text="RAPTIVA"/></sentence><sentence id="DrugDDI.d523.s1" origId="s1" text="RAPTIVA should not be used with other immunosuppressive drugs."><entity id="DrugDDI.d523.s1.e0" origId="s1.p5" charOffset="0-7" type="drug" text="RAPTIVA"/><entity id="DrugDDI.d523.s1.e1" origId="s1.p10" charOffset="38-55" type="drug" text="immunosuppressive"/><pair id="DrugDDI.d523.s1.p0" e1="DrugDDI.d523.s1.e0" e2="DrugDDI.d523.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d523.s2" origId="s2" text="Acellular, live and live-attenuated vaccines should not be administered during RAPTIVA treatment."><entity id="DrugDDI.d523.s2.e0" origId="s2.p13" charOffset="20-44" type="drug" text="live-attenuated vaccines"/><entity id="DrugDDI.d523.s2.e1" origId="s2.p18" charOffset="79-86" type="drug" text="RAPTIVA"/><pair id="DrugDDI.d523.s2.p0" e1="DrugDDI.d523.s2.e0" e2="DrugDDI.d523.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d523.s3" origId="s3" text="Drug/Laboratory Test Interactions Increases in lymphocyte counts related to the pharmacologic mechanism of action are frequently observed during RAPTIVA treatment."><entity id="DrugDDI.d523.s3.e0" origId="s3.p19" charOffset="0-4" type="drug" text="Drug"/><entity id="DrugDDI.d523.s3.e1" origId="s3.p29" charOffset="145-152" type="drug" text="RAPTIVA"/><pair id="DrugDDI.d523.s3.p0" e1="DrugDDI.d523.s3.e0" e2="DrugDDI.d523.s3.e1" interaction="false"/></sentence></document>