<document id="DrugDDI.d293" origId="Azatadine"><sentence id="DrugDDI.d293.s0" origId="s0" text="MAO inhibitors prolong and intensify the effects of antihistamines."><entity id="DrugDDI.d293.s0.e0" origId="s0.p0" charOffset="0-14" type="drug" text="MAO inhibitors"/><entity id="DrugDDI.d293.s0.e1" origId="s0.p5" charOffset="52-66" type="drug" text="antihistamines"/><pair id="DrugDDI.d293.s0.p0" e1="DrugDDI.d293.s0.e0" e2="DrugDDI.d293.s0.e1" interaction="true"/></sentence><sentence id="DrugDDI.d293.s1" origId="s1" text="Concomitant use of antihistamines with alcohol, tricyclic antidepressants, barbiturates, or other central nervous system depressants may have an additive effect."><entity id="DrugDDI.d293.s1.e0" origId="s1.p7" charOffset="19-33" type="drug" text="antihistamines"/><entity id="DrugDDI.d293.s1.e1" origId="s1.p8" charOffset="39-46" type="drug" text="alcohol"/><entity id="DrugDDI.d293.s1.e2" origId="s1.p9" charOffset="48-73" type="drug" text="tricyclic antidepressants"/><entity id="DrugDDI.d293.s1.e3" origId="s1.p10" charOffset="75-87" type="drug" text="barbiturates"/><entity id="DrugDDI.d293.s1.e4" origId="s1.p12" charOffset="98-132" type="drug" text="central nervous system depressants"/><pair id="DrugDDI.d293.s1.p0" e1="DrugDDI.d293.s1.e0" e2="DrugDDI.d293.s1.e1" interaction="true"/><pair id="DrugDDI.d293.s1.p1" e1="DrugDDI.d293.s1.e0" e2="DrugDDI.d293.s1.e2" interaction="false"/><pair id="DrugDDI.d293.s1.p2" e1="DrugDDI.d293.s1.e0" e2="DrugDDI.d293.s1.e3" interaction="true"/><pair id="DrugDDI.d293.s1.p3" e1="DrugDDI.d293.s1.e0" e2="DrugDDI.d293.s1.e4" interaction="false"/><pair id="DrugDDI.d293.s1.p4" e1="DrugDDI.d293.s1.e1" e2="DrugDDI.d293.s1.e2" interaction="false"/><pair id="DrugDDI.d293.s1.p5" e1="DrugDDI.d293.s1.e1" e2="DrugDDI.d293.s1.e3" interaction="false"/><pair id="DrugDDI.d293.s1.p6" e1="DrugDDI.d293.s1.e1" e2="DrugDDI.d293.s1.e4" interaction="false"/><pair id="DrugDDI.d293.s1.p7" e1="DrugDDI.d293.s1.e2" e2="DrugDDI.d293.s1.e3" interaction="false"/><pair id="DrugDDI.d293.s1.p8" e1="DrugDDI.d293.s1.e2" e2="DrugDDI.d293.s1.e4" interaction="false"/><pair id="DrugDDI.d293.s1.p9" e1="DrugDDI.d293.s1.e3" e2="DrugDDI.d293.s1.e4" interaction="false"/></sentence><sentence id="DrugDDI.d293.s2" origId="s2" text="When sympathomimetic drugs are given to patients receiving monoamine oxidase inhibitors, hypertensive reactions, including hypertensive crises, may occur."><entity id="DrugDDI.d293.s2.e0" origId="s2.p17" charOffset="5-26" type="drug" text="sympathomimetic drugs"/><entity id="DrugDDI.d293.s2.e1" origId="s2.p22" charOffset="59-87" type="drug" text="monoamine oxidase inhibitors"/><pair id="DrugDDI.d293.s2.p0" e1="DrugDDI.d293.s2.e0" e2="DrugDDI.d293.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d293.s3" origId="s3" text="The antihypertensive effects of methyldopa, mecamylamine, reserpine, and veratrum alkaloids may be reduced by sympathomimetics."><entity id="DrugDDI.d293.s3.e0" origId="s3.p28" charOffset="4-20" type="drug" text="antihypertensive"/><entity id="DrugDDI.d293.s3.e1" origId="s3.p29" charOffset="32-42" type="drug" text="methyldopa"/><entity id="DrugDDI.d293.s3.e2" origId="s3.p30" charOffset="44-56" type="drug" text="mecamylamine"/><entity id="DrugDDI.d293.s3.e3" origId="s3.p31" charOffset="58-67" type="drug" text="reserpine"/><entity id="DrugDDI.d293.s3.e4" origId="s3.p33" charOffset="73-91" type="drug" text="veratrum alkaloids"/><entity id="DrugDDI.d293.s3.e5" origId="s3.p37" charOffset="110-126" type="drug" text="sympathomimetics"/><pair id="DrugDDI.d293.s3.p0" e1="DrugDDI.d293.s3.e0" e2="DrugDDI.d293.s3.e1" interaction="false"/><pair id="DrugDDI.d293.s3.p1" e1="DrugDDI.d293.s3.e0" e2="DrugDDI.d293.s3.e2" interaction="false"/><pair id="DrugDDI.d293.s3.p2" e1="DrugDDI.d293.s3.e0" e2="DrugDDI.d293.s3.e3" interaction="false"/><pair id="DrugDDI.d293.s3.p3" e1="DrugDDI.d293.s3.e0" e2="DrugDDI.d293.s3.e4" interaction="false"/><pair id="DrugDDI.d293.s3.p4" e1="DrugDDI.d293.s3.e0" e2="Drug