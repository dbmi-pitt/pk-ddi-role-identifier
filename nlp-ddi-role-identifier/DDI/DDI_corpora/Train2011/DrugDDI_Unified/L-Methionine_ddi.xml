<document id="DrugDDI.d231" origId="L-Methionine"><sentence id="DrugDDI.d231.s0" origId="s0" text="Acetaminophen and methotrexate - L-methionine may decrease hepatic toxicity in those with acetaminophen overdosage or in those taking methotrexate."><entity id="DrugDDI.d231.s0.e0" origId="s0.p0" charOffset="0-13" type="drug" text="Acetaminophen"/><entity id="DrugDDI.d231.s0.e1" origId="s0.p2" charOffset="18-30" type="drug" text="methotrexate"/><entity id="DrugDDI.d231.s0.e2" origId="s0.p6" charOffset="90-103" type="drug" text="acetaminophen"/><entity id="DrugDDI.d231.s0.e3" origId="s0.p8" charOffset="134-146" type="drug" text="methotrexate"/><pair id="DrugDDI.d231.s0.p0" e1="DrugDDI.d231.s0.e0" e2="DrugDDI.d231.s0.e1" interaction="false"/><pair id="DrugDDI.d231.s0.p1" e1="DrugDDI.d231.s0.e0" e2="DrugDDI.d231.s0.e2" interaction="false"/><pair id="DrugDDI.d231.s0.p2" e1="DrugDDI.d231.s0.e0" e2="DrugDDI.d231.s0.e3" interaction="false"/><pair id="DrugDDI.d231.s0.p3" e1="DrugDDI.d231.s0.e1" e2="DrugDDI.d231.s0.e2" interaction="true"/><pair id="DrugDDI.d231.s0.p4" e1="DrugDDI.d231.s0.e1" e2="DrugDDI.d231.s0.e3" interaction="false"/><pair id="DrugDDI.d231.s0.p5" e1="DrugDDI.d231.s0.e2" e2="DrugDDI.d231.s0.e3" interaction="false"/></sentence><sentence id="DrugDDI.d231.s1" origId="s1" text="Theoretically, it may decrease hepatic toxicity in the case of other potential hepatotoxic drugs, as well."><entity id="DrugDDI.d231.s1.e0" origId="s1.p15" charOffset="91-96" type="drug" text="drugs"/></sentence><sentence id="DrugDDI.d231.s2" origId="s2" text="Gentamicin - Methionine may protect against the ototoxic effects of gentamicin."><entity id="DrugDDI.d231.s2.e0" origId="s2.p17" charOffset="0-10" type="drug" text="Gentamicin"/><entity id="DrugDDI.d231.s2.e1" origId="s2.p21" charOffset="68-78" type="drug" text="gentamicin"/><pair id="DrugDDI.d231.s2.p0" e1="DrugDDI.d231.s2.e0" e2="DrugDDI.d231.s2.e1" interaction="true"/></sentence></document>