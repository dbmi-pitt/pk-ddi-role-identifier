<document id="DrugDDI.d402" origId="Epinephrine"><sentence id="DrugDDI.d402.s0" origId="s0" text="Epinephrine should be used cautiously in patients with hyperthyroidism, hypertension and cardiac arrhythmias."><entity id="DrugDDI.d402.s0.e0" origId="s0.p0" charOffset="0-11" type="drug" text="Epinephrine"/></sentence><sentence id="DrugDDI.d402.s1" origId="s1" text="All vasopressors should be used cautiously in patients taking monoamine oxidase (MAO) inhibitors."><entity id="DrugDDI.d402.s1.e0" origId="s1.p10" charOffset="4-15" type="drug" text="vasopressor"/><entity id="DrugDDI.d402.s1.e1" origId="s1.p17" charOffset="62-79" type="drug" text="monoamine oxidase"/><pair id="DrugDDI.d402.s1.p0" e1="DrugDDI.d402.s1.e0" e2="DrugDDI.d402.s1.e1" interaction="true"/></sentence><sentence id="DrugDDI.d402.s2" origId="s2" text="Epinephrine should not be administered concomitantly with other sympathomimetic drugs (such as isoproterenol) because of possible additive effects and increased toxicity."><entity id="DrugDDI.d402.s2.e0" origId="s2.p22" charOffset="0-11" type="drug" text="Epinephrine"/><entity id="DrugDDI.d402.s2.e1" origId="s2.p28" charOffset="64-85" type="drug" text="sympathomimetic drugs"/><entity id="DrugDDI.d402.s2.e2" origId="s2.p30" charOffset="95-108" type="drug" text="isoproterenol"/><pair id="DrugDDI.d402.s2.p0" e1="DrugDDI.d402.s2.e0" e2="DrugDDI.d402.s2.e1" interaction="true"/><pair id="DrugDDI.d402.s2.p1" e1="DrugDDI.d402.s2.e0" e2="DrugDDI.d402.s2.e2" interaction="true"/><pair id="DrugDDI.d402.s2.p2" e1="DrugDDI.d402.s2.e1" e2="DrugDDI.d402.s2.e2" interaction="false"/></sentence><sentence id="DrugDDI.d402.s3" origId="s3" text="Combined effects may induce serious cardiac arrhythmias."/><sentence id="DrugDDI.d402.s4" origId="s4" text="They may be administered alternately when the preceding effect of other such drug has subsided."><entity id="DrugDDI.d402.s4.e0" origId="s4.p46" charOffset="77-81" type="drug" text="drug"/></sentence><sentence id="DrugDDI.d402.s5" origId="s5" text="Administration of epinephrine to patients receiving cyclopropane or halogenated hydrocarbon general anesthetics such as halothane which sensitize the myocardium, may induce cardiac arrhythmia.."><entity id="DrugDDI.d402.s5.e0" origId="s5.p50" charOffset="18-29" type="drug" text="epinephrine"/><entity id="DrugDDI.d402.s5.e1" origId="s5.p53" charOffset="52-64" type="drug" text="cyclopropane"/><entity id="DrugDDI.d402.s5.e2" origId="s5.p55" charOffset="68-111" type="drug" text="halogenated hydrocarbon general anesthetics"/><entity id="DrugDDI.d402.s5.e3" origId="s5.p56" charOffset="120-129" type="drug" text="halothane"/><pair id="DrugDDI.d402.s5.p0" e1="DrugDDI.d402.s5.e0" e2="DrugDDI.d402.s5.e1" interaction="true"/><pair id="DrugDDI.d402.s5.p1" e1="DrugDDI.d402.s5.e0" e2="DrugDDI.d402.s5.e2" interaction="true"/><pair id="DrugDDI.d402.s5.p2" e1="DrugDDI.d402.s5.e0" e2="DrugDDI.d402.s5.e3" interaction="true"/><pair id="DrugDDI.d402.s5.p3" e1="DrugDDI.d402.s5.e1" e2="DrugDDI.d402.s5.e2" interaction="false"/><pair id="DrugDDI.d402.s5.p4" e1="DrugDDI.d402.s5.e1" e2="DrugDDI.d402.s5.e3" interaction="false"/><pair id="DrugDDI.d402.s5.p5" e1="DrugDDI.d402.s5.e2" e2="DrugDDI.d402.s5.e3" interaction="false"/></sentence><sentence id="DrugDDI.d402.s6" origId="s6" text="When encountered, such arrhythmias may respond to administration of a beta-adrenergic blocking drug."><entity id="DrugDDI.d402.s6.e0" origId="s6.p70" charOffset="70-99" type="drug" text="beta-adrenergic blocking drug"/></sentence><sentence id="DrugDDI.d402.s7" origId="s7" text="Epinephrine also should be used cautiously with other drugs (e.g., digitalis, glycosides) that sensitize the myocardium to the actions of sympathomimetic drugs."><entity id="DrugDDI.d402.s7.e0" origId="s7.p71" charOffset="0-11" type="drug" text="Epinephrine"/><entity id="DrugDDI.d402.s7.e1" origId="s7.p76" charOffset="54-59" type="drug" text="drugs"/><entity id="DrugDDI.d402.s7.e2" origId="s7.p79" charOffset="67-76" type="drug" text="digitalis"/><entity id="DrugDDI.d402.s7.e3" origId="s7.p86" charOffset="138-159" type="drug" text="sympathomimetic drugs"/><pair id="DrugDDI.d402.s7.p0" e1="DrugDDI.d402.s7.e0" e2="DrugDDI.d402.s7.e1" interaction="true"/><pair id="DrugDDI.d402.s7.p1" e1="DrugDDI.d402.s7.e0" e2="DrugDDI.d402.s7.e2" interaction="true"/><pair id="DrugDDI.d402.s7.p2" e1="DrugDDI.d402.s7.e0" e2="DrugDDI.d402.s7.e3" interaction="false"/><pair id="DrugDDI.d402.s7.p3" e1="DrugDDI.d402.s7.e1" e2="DrugDDI.d402.s7.e2" interaction="false"/><pair id="DrugDDI.d402.s7.p4" e1="DrugDDI.d402.s7.e1" e2="DrugDDI.d402.s7.e3" interaction="false"/><pair id="DrugDDI.d402.s7.p5" e1="DrugDDI.d402.s7.e2" e2="DrugDDI.d402.s7.e3" interaction="false"/></sentence><sentence id="DrugDDI.d402.s8" origId="s8" text="Diuretic agents may decrease vascular response to pressor drugs such as epinephrine."><entity id="DrugDDI.d402.s8.e0" origId="s8.p87" charOffset="0-8" type="drug" text="Diuretic"/><entity id="DrugDDI.d402.s8.e1" origId="s8.p91" charOffset="50-63" type="drug" text="pressor drugs"/><entity id="DrugDDI.d402.s8.e2" origId="s8.p92" charOffset="72-83" type="drug" text="epinephrine"/><pair id="DrugDDI.d402.s8.p0" e1="DrugDDI.d402.s8.e0" e2="DrugDDI.d402.s8.e1" interaction="true"/><pair id="DrugDDI.d402.s8.p1" e1="DrugDDI.d402.s8.e0" e2="DrugDDI.d402.s8.e2" interaction="true"/><pair id="DrugDDI.d402.s8.p2" e1="DrugDDI.d402.s8.e1" e2="DrugDDI.d402.s8.e2" interaction="false"/></sentence><sentence id="DrugDDI.d402.s9" origId="s9" text="Epinephrine may antagonize the neuron blockade produced by guanethidine resulting in decreased antihypertensive effect and requiring increased dosage of the latter."><entity id="DrugDDI.d402.s9.e0" origId="s9.p93" charOffset="0-11" type="drug" text="Epinephrine"/><entity id="DrugDDI.d402.s9.e1" origId="s9.p98" charOffset="59-71" type="drug" text="guanethidine"/><entity id="DrugDDI.d402.s9.e2" origId="s9.p100" charOffset="95-111" type="drug" text="antihypertensive"/><pair id="DrugDDI.d402.s9.p0" e1="DrugDDI.d402.s9.e0" e2="DrugDDI.d402.s9.e1" interaction="true"/><pair id="DrugDDI.d402.s9.p1" e1="DrugDDI.d402.s9.e0" e2="DrugDDI.d402.s9.e2" interaction="false"/><pair id="DrugDDI.d402.s9.p2" e1="DrugDDI.d402.s9.e1" e2="DrugDDI.d402.s9.e2" interaction="false"/></sentence></document>