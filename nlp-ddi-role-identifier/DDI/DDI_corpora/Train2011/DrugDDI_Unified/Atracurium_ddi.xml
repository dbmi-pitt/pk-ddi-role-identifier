<document id="DrugDDI.d494" origId="Atracurium"><sentence id="DrugDDI.d494.s0" origId="s0" text="Drugs which may enhance the neuromuscular blocking action of TRACRIUM include: enflurane;"><entity id="DrugDDI.d494.s0.e0" origId="s0.p0" charOffset="0-5" type="drug" text="Drugs"/><entity id="DrugDDI.d494.s0.e1" origId="s0.p5" charOffset="61-69" type="drug" text="TRACRIUM"/><entity id="DrugDDI.d494.s0.e2" origId="s0.p8" charOffset="79-88" type="drug" text="enflurane"/><pair id="DrugDDI.d494.s0.p0" e1="DrugDDI.d494.s0.e0" e2="DrugDDI.d494.s0.e1" interaction="true"/><pair id="DrugDDI.d494.s0.p1" e1="DrugDDI.d494.s0.e0" e2="DrugDDI.d494.s0.e2" interaction="false"/><pair id="DrugDDI.d494.s0.p2" e1="DrugDDI.d494.s0.e1" e2="DrugDDI.d494.s0.e2" interaction="true"/></sentence><sentence id="DrugDDI.d494.s1" origId="s1" text="isoflurane;"><entity id="DrugDDI.d494.s1.e0" origId="s1.p10" charOffset="0-10" type="drug" text="isoflurane"/></sentence><sentence id="DrugDDI.d494.s2" origId="s2" text="halothane;"><entity id="DrugDDI.d494.s2.e0" origId="s2.p12" charOffset="0-9" type="drug" text="halothane"/></sentence><sentence id="DrugDDI.d494.s3" origId="s3" text="certain antibiotics, especially the aminoglycosides and polymyxins;"><entity id="DrugDDI.d494.s3.e0" origId="s3.p14" charOffset="8-19" type="drug" text="antibiotics"/><entity id="DrugDDI.d494.s3.e1" origId="s3.p15" charOffset="36-51" type="drug" text="aminoglycosides"/><entity id="DrugDDI.d494.s3.e2" origId="s3.p17" charOffset="56-66" type="drug" text="polymyxins"/><pair id="DrugDDI.d494.s3.p0" e1="DrugDDI.d494.s3.e0" e2="DrugDDI.d494.s3.e1" interaction="false"/><pair id="DrugDDI.d494.s3.p1" e1="DrugDDI.d494.s3.e0" e2="DrugDDI.d494.s3.e2" interaction="false"/><pair id="DrugDDI.d494.s3.p2" e1="DrugDDI.d494.s3.e1" e2="DrugDDI.d494.s3.e2" interaction="false"/></sentence><sentence id="DrugDDI.d494.s4" origId="s4" text="lithium;"><entity id="DrugDDI.d494.s4.e0" origId="s4.p19" charOffset="0-7" type="drug" text="lithium"/></sentence><sentence id="DrugDDI.d494.s5" origId="s5" text="magnesium salts;"><entity id="DrugDDI.d494.s5.e0" origId="s5.p21" charOffset="0-14" type="drug" text="magnesium salt"/></sentence><sentence id="DrugDDI.d494.s6" origId="s6" text="procainamide;"><entity id="DrugDDI.d494.s6.e0" origId="s6.p23" charOffset="0-12" type="drug" text="procainamide"/></sentence><sentence id="DrugDDI.d494.s7" origId="s7" text="and quinidine."><entity id="DrugDDI.d494.s7.e0" origId="s7.p26" charOffset="4-13" type="drug" text="quinidine"/></sentence><sentence id="DrugDDI.d494.s8" origId="s8" text="If other muscle relaxants are used during the same procedure, the possibility of a synergistic or antagonist effect should be considered."><entity id="DrugDDI.d494.s8.e0" origId="s8.p27" charOffset="9-25" type="drug" text="muscle relaxants"/></sentence><sentence id="DrugDDI.d494.s9" origId="s9" text="The prior administration of succinylcholine does not enhance the duration, but quickens the onset and may increase the depth, of neuromuscular block induced by TRACRIUM."><entity id="DrugDDI.d494.s9.e0" origId="s9.p39" charOffset="28-43" type="drug" text="succinylcholine"/><entity id="DrugDDI.d494.s9.e1" origId="s9.p53" charOffset="160-168" type="drug" text="TRACRIUM"/><pair id="DrugDDI.d494.s9.p0" e1="DrugDDI.d494.s9.e0" e2="DrugDDI.d494.s9.e1" interaction="true"/></sentence><sentence id="DrugDDI.d494.s10" origId="s10" text="TRACRIUM should not be administered until a patient has recovered from succinylcholine-induced neuromuscular block."><entity id="DrugDDI.d494.s10.e0" origId="s10.p54" charOffset="0-8" type="drug" text="TRACRIUM"/><entity id="DrugDDI.d494.s10.e1" origId="s10.p62" charOffset="71-86" type="drug" text="succinylcholine"/><pair id="DrugDDI.d494.s10.p0" e1="DrugDDI.d494.s10.e0" e2="DrugDDI.d494.s10.e1" interaction="true"/></sentence></document>