<document id="DrugDDI.d535" origId="Auranofin"><sentence id="DrugDDI.d535.s0" origId="s0" text="Auranofin should be avoided by patients with a history of serious reaction to any gold medication, including Solganal and Myochrysine."><entity id="DrugDDI.d535.s0.e0" origId="s0.p0" charOffset="0-9" type="drug" text="Auranofin"/><entity id="DrugDDI.d535.s0.e1" origId="s0.p7" charOffset="87-98" type="drug" text="medication"/><entity id="DrugDDI.d535.s0.e2" origId="s0.p9" charOffset="109-117" type="drug" text="Solganal"/><entity id="DrugDDI.d535.s0.e3" origId="s0.p11" charOffset="122-133" type="drug" text="Myochrysine"/><pair id="DrugDDI.d535.s0.p0" e1="DrugDDI.d535.s0.e0" e2="DrugDDI.d535.s0.e1" interaction="true"/><pair id="DrugDDI.d535.s0.p1" e1="DrugDDI.d535.s0.e0" e2="DrugDDI.d535.s0.e2" interaction="false"/><pair id="DrugDDI.d535.s0.p2" e1="DrugDDI.d535.s0.e0" e2="DrugDDI.d535.s0.e3" interaction="true"/><pair id="DrugDDI.d535.s0.p3" e1="DrugDDI.d535.s0.e1" e2="DrugDDI.d535.s0.e2" interaction="false"/><pair id="DrugDDI.d535.s0.p4" e1="DrugDDI.d535.s0.e1" e2="DrugDDI.d535.s0.e3" interaction="false"/><pair id="DrugDDI.d535.s0.p5" e1="DrugDDI.d535.s0.e2" e2="DrugDDI.d535.s0.e3" interaction="false"/></sentence><sentence id="DrugDDI.d535.s1" origId="s1" text="Auranofin should not be used together with penicillamine (Depen, Cuprimine), another arthritis medication."><entity id="DrugDDI.d535.s1.e0" origId="s1.p12" charOffset="0-9" type="drug" text="Auranofin"/><entity id="DrugDDI.d535.s1.e1" origId="s1.p17" charOffset="43-56" type="drug" text="penicillamine"/><entity id="DrugDDI.d535.s1.e2" origId="s1.p19" charOffset="58-63" type="drug" text="Depen"/><entity id="DrugDDI.d535.s1.e3" origId="s1.p20" charOffset="65-74" type="drug" text="Cuprimine"/><entity id="DrugDDI.d535.s1.e4" origId="s1.p23" charOffset="85-106" type="drug" text="arthritis medication"/><pair id="DrugDDI.d535.s1.p0" e1="DrugDDI.d535.s1.e0" e2="DrugDDI.d535.s1.e1" interaction="true"/><pair id="DrugDDI.d535.s1.p1" e1="DrugDDI.d535.s1.e0" e2="DrugDDI.d535.s1.e2" interaction="false"/><pair id="DrugDDI.d535.s1.p2" e1="DrugDDI.d535.s1.e0" e2="DrugDDI.d535.s1.e3" interaction="true"/><pair id="DrugDDI.d535.s1.p3" e1="DrugDDI.d535.s1.e0" e2="DrugDDI.d535.s1.e4" interaction="false"/><pair id="DrugDDI.d535.s1.p4" e1="DrugDDI.d535.s1.e1" e2="DrugDDI.d535.s1.e2" interaction="false"/><pair id="DrugDDI.d535.s1.p5" e1="DrugDDI.d535.s1.e1" e2="DrugDDI.d535.s1.e3" interaction="false"/><pair id="DrugDDI.d535.s1.p6" e1="DrugDDI.d535.s1.e1" e2="DrugDDI.d535.s1.e4" interaction="false"/><pair id="DrugDDI.d535.s1.p7" e1="DrugDDI.d535.s1.e2" e2="DrugDDI.d535.s1.e3" interaction="false"/><pair id="DrugDDI.d535.s1.p8" e1="DrugDDI.d535.s1.e2" e2="DrugDDI.d535.s1.e4" interaction="false"/><pair id="DrugDDI.d535.s1.p9" e1="DrugDDI.d535.s1.e3" e2="DrugDDI.d535.s1.e4" interaction="false"/></sentence><sentence id="DrugDDI.d535.s2" origId="s2" text="It should also be avoided in patients with blood, liver or kidney diseases, recent radiation treatment, or uncontrolled diabetes."><entity id="DrugDDI.d535.s2.e0" origId="s2.p31" charOffset="50-55" type="drug" text="liver"/></sentence><sentence id="DrugDDI.d535.s3" origId="s3" text="Patients should report to their practitioners any new rashes, itching, mouth sores, or unusual taste while taking auranofin."><entity id="DrugDDI.d535.s3.e0" origId="s3.p47" charOffset="114-123" type="drug" text="auranofin"/></sentence><sentence id="DrugDDI.d535.s4" origId="s4" text="Gold is excreted slowly from the body."><entity id="DrugDDI.d535.s4.e0" origId="s4.p48" charOffset="0-4" type="drug" text="Gold"/></sentence><sentence id="DrugDDI.d535.s5" origId="s5" text="Safety and effectiveness in children has not been established."/></document>