<document id="DrugDDI.d542" origId="Hydroxyurea"><sentence id="DrugDDI.d542.s0" origId="s0" text="Prospective studies on the potential for hydroxyurea to interact with other drugs have not been performed."><entity id="DrugDDI.d542.s0.e0" origId="s0.p2" charOffset="41-52" type="drug" text="hydroxyurea"/><entity id="DrugDDI.d542.s0.e1" origId="s0.p4" charOffset="76-81" type="drug" text="drugs"/><pair id="DrugDDI.d542.s0.p0" e1="DrugDDI.d542.s0.e0" e2="DrugDDI.d542.s0.e1" interaction="false"/></sentence><sentence id="DrugDDI.d542.s1" origId="s1" text="Concurrent use of hydroxyurea and other myelosuppressive agents or radiation therapy may increase the likelihood of bone marrow depression or other adverse events."><entity id="DrugDDI.d542.s1.e0" origId="s1.p10" charOffset="18-29" type="drug" text="hydroxyurea"/></sentence><sentence id="DrugDDI.d542.s2" origId="s2" text="Since hydroxyurea may raise the serum uric acid level, dosage adjustment of uricosuric medication may be necessary"><entity id="DrugDDI.d542.s2.e0" origId="s2.p22" charOffset="6-17" type="drug" text="hydroxyurea"/><entity id="DrugDDI.d542.s2.e1" origId="s2.p27" charOffset="76-98" type="drug" text="uricosuric medication"/><pair id="DrugDDI.d542.s2.p0" e1="DrugDDI.d542.s2.e0" e2="DrugDDI.d542.s2.e1" interaction="true"/></sentence><sentence id="DrugDDI.d542.s3" origId="s3" text="."/><sentence id="DrugDDI.d542.s4" origId="s4" text="? ?"/></document>