<document id="DrugDDI.d572" origId="Ibutilide"><sentence id="DrugDDI.d572.s0" origId="s0" text="No specific pharmacokinetic or other formal drug interaction studies were conducted."/><sentence id="DrugDDI.d572.s1" origId="s1" text="Digoxin: Supraventricular arrhythmias may mask the cardiotoxicity associated with excessive digoxin levels."><entity id="DrugDDI.d572.s1.e0" origId="s1.p5" charOffset="0-7" type="drug" text="Digoxin"/></sentence><sentence id="DrugDDI.d572.s2" origId="s2" text="Therefore, it is advisable to be particularly cautious in patients whose plasma digoxin levels are above or suspected to be above the usual therapeutic range."/><sentence id="DrugDDI.d572.s3" origId="s3" text="Coadministration of digoxin did not have effects on either the safety or efficacy of ibutilide in the clinical trials."><entity id="DrugDDI.d572.s3.e0" origId="s3.p30" charOffset="20-27" type="drug" text="digoxin"/><entity id="DrugDDI.d572.s3.e1" origId="s3.p40" charOffset="85-94" type="drug" text="ibutilide"/><pair id="DrugDDI.d572.s3.p0" e1="DrugDDI.d572.s3.e0" e2="DrugDDI.d572.s3.e1" interaction="false"/></sentence><sentence id="DrugDDI.d572.s4" origId="s4" text="Calcium channel blocking agents: Coadministration of calcium channel blockers did not have any effect on either the safety or efficacy of ibutilide in the clinical trials."><entity id="DrugDDI.d572.s4.e0" origId="s4.p42" charOffset="0-31" type="drug" text="Calcium channel blocking agents"/><entity id="DrugDDI.d572.s4.e1" origId="s4.p45" charOffset="53-77" type="drug" text="calcium channel blockers"/><entity id="DrugDDI.d572.s4.e2" origId="s4.p55" charOffset="138-147" type="drug" text="ibutilide"/><pair id="DrugDDI.d572.s4.p0" e1="DrugDDI.d572.s4.e0" e2="DrugDDI.d572.s4.e1" interaction="false"/><pair id="DrugDDI.d572.s4.p1" e1="DrugDDI.d572.s4.e0" e2="DrugDDI.d572.s4.e2" interaction="false"/><pair id="DrugDDI.d572.s4.p2" e1="DrugDDI.d572.s4.e1" e2="DrugDDI.d572.s4.e2" interaction="false"/></sentence><sentence id="DrugDDI.d572.s5" origId="s5" text="Beta-adrenergic blocking agents: Coadministration of beta-adrenergic blocking agents did not have any effect on either the safety or efficacy of ibutilide in the clinical trials."><entity id="DrugDDI.d572.s5.e0" origId="s5.p57" charOffset="0-31" type="drug" text="Beta-adrenergic blocking agents"/><entity id="DrugDDI.d572.s5.e1" origId="s5.p60" charOffset="53-84" type="drug" text="beta-adrenergic blocking agents"/><entity id="DrugDDI.d572.s5.e2" origId="s5.p70" charOffset="145-154" type="drug" text="ibutilide"/><pair id="DrugDDI.d572.s5.p0" e1="DrugDDI.d572.s5.e0" e2="DrugDDI.d572.s5.e1" interaction="false"/><pair id="DrugDDI.d572.s5.p1" e1="DrugDDI.d572.s5.e0" e2="DrugDDI.d572.s5.e2" interaction="false"/><pair id="DrugDDI.d572.s5.p2" e1="DrugDDI.d572.s5.e1" e2="DrugDDI.d572.s5.e2" interaction="false"/></sentence></document>