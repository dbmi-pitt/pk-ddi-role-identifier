<document id="DrugDDI.d336" origId="Neomycin"><sentence id="DrugDDI.d336.s0" origId="s0" text="Caution should be taken in concurrent or serial use of other neurotoxic and/ or nephrotoxic drugs because of possible enhancement of the nephrotoxicity and/or ototoxicity of neomycin."><entity id="DrugDDI.d336.s0.e0" origId="s0.p9" charOffset="92-97" type="drug" text="drugs"/><entity id="DrugDDI.d336.s0.e1" origId="s0.p14" charOffset="174-182" type="drug" text="neomycin"/><pair id="DrugDDI.d336.s0.p0" e1="DrugDDI.d336.s0.e0" e2="DrugDDI.d336.s0.e1" interaction="?"/></sentence><sentence id="DrugDDI.d336.s1" origId="s1" text="Caution should also be taken in concurrent or serial use of other aminoglycosides and polymyxins because they may enhance neomycin?s nephrotoxicity and/or ototoxicity and potentiate neomycin sulfate?s neuromuscular blocking effects."><entity id="DrugDDI.d336.s1.e0" origId="s1.p23" charOffset="66-81" type="drug" text="aminoglycosides"/><entity id="DrugDDI.d336.s1.e1" origId="s1.p25" charOffset="86-96" type="drug" text="polymyxins"/><entity id="DrugDDI.d336.s1.e2" origId="s1.p35" charOffset="182-190" type="drug" text="neomycin"/><pair id="DrugDDI.d336.s1.p0" e1="DrugDDI.d336.s1.e0" e2="DrugDDI.d336.s1.e1" interaction="?"/><pair id="DrugDDI.d336.s1.p1" e1="DrugDDI.d336.s1.e0" e2="DrugDDI.d336.s1.e2" interaction="?"/><pair id="DrugDDI.d336.s1.p2" e1="DrugDDI.d336.s1.e1" e2="DrugDDI.d336.s1.e2" interaction="?"/></sentence><sentence id="DrugDDI.d336.s2" origId="s2" text="Oral neomycin inhibits the gastrointestinal absorption of penicillin V, oral vitamin B-12, methotrexate and 5-fluorouracil."><entity id="DrugDDI.d336.s2.e0" origId="s2.p36" charOffset="0-13" type="drug" text="Oral neomycin"/><entity id="DrugDDI.d336.s2.e1" origId="s2.p39" charOffset="58-70" type="drug" text="penicillin V"/><entity id="DrugDDI.d336.s2.e2" origId="s2.p40" charOffset="72-90" type="drug" text="oral vitamin B-12"/><entity id="DrugDDI.d336.s2.e3" origId="s2.p41" charOffset="91-103" type="drug" text="methotrexate"/><entity id="DrugDDI.d336.s2.e4" origId="s2.p43" charOffset="108-122" type="drug" text="5-fluorouracil"/><pair id="DrugDDI.d336.s2.p0" e1="DrugDDI.d336.s2.e0" e2="DrugDDI.d336.s2.e1" interaction="?"/><pair id="DrugDDI.d336.s2.p1" e1="DrugDDI.d336.s2.e0" e2="DrugDDI.d336.s2.e2" interaction="?"/><pair id="DrugDDI.d336.s2.p2" e1="DrugDDI.d336.s2.e0" e2="DrugDDI.d336.s2.e3" interaction="?"/><pair id="DrugDDI.d336.s2.p3" e1="DrugDDI.d336.s2.e0" e2="DrugDDI.d336.s2.e4" interaction="?"/><pair id="DrugDDI.d336.s2.p4" e1="DrugDDI.d336.s2.e1" e2="DrugDDI.d336.s2.e2" interaction="?"/><pair id="DrugDDI.d336.s2.p5" e1="DrugDDI.d336.s2.e1" e2="DrugDDI.d336.s2.e3" interaction="?"/><pair id="DrugDDI.d336.s2.p6" e1="DrugDDI.d336.s2.e1" e2="DrugDDI.d336.s2.e4" interaction="?"/><pair id="DrugDDI.d336.s2.p7" e1="DrugDDI.d336.s2.e2" e2="DrugDDI.d336.s2.e3" interaction="?"/><pair id="DrugDDI.d336.s2.p8" e1="DrugDDI.d336.s2.e2" e2="DrugDDI.d336.s2.e4" interaction="?"/><pair id="DrugDDI.d336.s2.p9" e1="DrugDDI.d336.s2.e3" e2="DrugDDI.d336.s2.e4" interaction="?"/></sentence><sentence id="DrugDDI.d336.s3" origId="s3" text="The gastrointestinal absorption of digoxin also appears to be inhibited."><entity id="DrugDDI.d336.s3.e0" origId="s3.p45" charOffset="35-42" type="drug" text="digoxin"/></sentence><sentence id="DrugDDI.d336.s4" origId="s4" text="Therefore, digoxin serum levels should be monitored."/><sentence id="DrugDDI.d336.s5" origId="s5" text="Oral neomycin sulfate may enhance the effect of coumarin in anticoagulants by decreasing vitamin K availability."><entity id="DrugDDI.d336.s5.e0" origId="s5.p55" charOffset="0-13" type="drug" text="Oral neomycin"/><entity id="DrugDDI.d336.s5.e1" origId="s5.p59" charOffset="48-56" type="drug" text="coumarin"/><entity id="DrugDDI.d336.s5.e2" origId="s5.p60" charOffset="60-74" type="drug" text="anticoagulants"/><pair id="DrugDDI.d336.s5.p0" e1="DrugDDI.d336.s5.e0" e2="DrugDDI.d336.s5.e1" interaction="?"/><pair id="DrugDDI.d336.s5.p1" e1="DrugDDI.d336.s5.e0" e2="DrugDDI.d336.s5.e2" interaction="?"/><pair id="DrugDDI.d336.s5.p2" e1="DrugDDI.d336.s5.e1" e2="DrugDDI.d336.s5.e2" interaction="?"/></sentence></document>