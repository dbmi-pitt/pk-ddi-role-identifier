<document id="DrugDDI.d433" origId="Fluoxymesterone"><sentence id="DrugDDI.d433.s0" origId="s0" text="Androgens may increase sensitivity to oral anticoagulahts."><entity id="DrugDDI.d433.s0.e0" origId="s0.p0" charOffset="0-9" type="drug" text="Androgens"/></sentence><sentence id="DrugDDI.d433.s1" origId="s1" text="Dosage of the anticoagulant may require reduction in order to maintain satisfactory therapeutic hypoprothrombinemia."><entity id="DrugDDI.d433.s1.e0" origId="s1.p6" charOffset="14-27" type="drug" text="anticoagulant"/></sentence><sentence id="DrugDDI.d433.s2" origId="s2" text="Concurrent administration of oxyphenbutazone and androgens may result in elevated serum levels of oxyphenbutazone."><entity id="DrugDDI.d433.s2.e0" origId="s2.p14" charOffset="29-44" type="drug" text="oxyphenbutazone"/><entity id="DrugDDI.d433.s2.e1" origId="s2.p16" charOffset="49-58" type="drug" text="androgens"/><entity id="DrugDDI.d433.s2.e2" origId="s2.p20" charOffset="98-113" type="drug" text="oxyphenbutazone"/><pair id="DrugDDI.d433.s2.p0" e1="DrugDDI.d433.s2.e0" e2="DrugDDI.d433.s2.e1" interaction="?"/><pair id="DrugDDI.d433.s2.p1" e1="DrugDDI.d433.s2.e0" e2="DrugDDI.d433.s2.e2" interaction="?"/><pair id="DrugDDI.d433.s2.p2" e1="DrugDDI.d433.s2.e1" e2="DrugDDI.d433.s2.e2" interaction="?"/></sentence><sentence id="DrugDDI.d433.s3" origId="s3" text="In diabetic patients, the metabolic effects of androgens may decrease blood glucose and therefore, insulin requirements."><entity id="DrugDDI.d433.s3.e0" origId="s3.p23" charOffset="47-56" type="drug" text="androgens"/><entity id="DrugDDI.d433.s3.e1" origId="s3.p29" charOffset="99-106" type="drug" text="insulin"/><pair id="DrugDDI.d433.s3.p0" e1="DrugDDI.d433.s3.e0" e2="DrugDDI.d433.s3.e1" interaction="?"/></sentence></document>