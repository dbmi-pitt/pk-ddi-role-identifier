<document id="DrugDDI.d51" origId="Daunorubicin"><sentence id="DrugDDI.d51.s0" origId="s0" text="Use of Cerubidine in a patient who has previously received doxorubicin increases the risk of cardiotoxicity."><entity id="DrugDDI.d51.s0.e0" origId="s0.p1" charOffset="7-17" type="drug" text="Cerubidine"/><entity id="DrugDDI.d51.s0.e1" origId="s0.p5" charOffset="59-70" type="drug" text="doxorubicin"/><pair id="DrugDDI.d51.s0.p0" e1="DrugDDI.d51.s0.e0" e2="DrugDDI.d51.s0.e1" interaction="?"/></sentence><sentence id="DrugDDI.d51.s1" origId="s1" text="Cerubidine should not be used in patients who have previously received the recommended maximum cumulative doses of doxorubicin or Cerubidine."><entity id="DrugDDI.d51.s1.e0" origId="s1.p9" charOffset="0-10" type="drug" text="Cerubidine"/><entity id="DrugDDI.d51.s1.e1" origId="s1.p20" charOffset="115-126" type="drug" text="doxorubicin"/><entity id="DrugDDI.d51.s1.e2" origId="s1.p22" charOffset="130-140" type="drug" text="Cerubidine"/><pair id="DrugDDI.d51.s1.p0" e1="DrugDDI.d51.s1.e0" e2="DrugDDI.d51.s1.e1" interaction="?"/><pair id="DrugDDI.d51.s1.p1" e1="DrugDDI.d51.s1.e0" e2="DrugDDI.d51.s1.e2" interaction="?"/><pair id="DrugDDI.d51.s1.p2" e1="DrugDDI.d51.s1.e1" e2="DrugDDI.d51.s1.e2" interaction="?"/></sentence><sentence id="DrugDDI.d51.s2" origId="s2" text="Cyclophosphamide used concurrently with Cerubidine may also result in increased cardiotoxicity."><entity id="DrugDDI.d51.s2.e0" origId="s2.p23" charOffset="0-16" type="drug" text="Cyclophosphamide"/><entity id="DrugDDI.d51.s2.e1" origId="s2.p26" charOffset="40-50" type="drug" text="Cerubidine"/><pair id="DrugDDI.d51.s2.p0" e1="DrugDDI.d51.s2.e0" e2="DrugDDI.d51.s2.e1" interaction="?"/></sentence><sentence id="DrugDDI.d51.s3" origId="s3" text="Dosage reduction of Cerubidine may be required when used concurrently with other myelosuppressive agents."><entity id="DrugDDI.d51.s3.e0" origId="s3.p32" charOffset="20-30" type="drug" text="Cerubidine"/></sentence><sentence id="DrugDDI.d51.s4" origId="s4" text="Hepatotoxic medications, such as high-dose methotrexate, may impair liver function and increase the risk of toxicity."><entity id="DrugDDI.d51.s4.e0" origId="s4.p40" charOffset="12-23" type="drug" text="medications"/><entity id="DrugDDI.d51.s4.e1" origId="s4.p41" charOffset="43-55" type="drug" text="methotrexate"/><pair id="DrugDDI.d51.s4.p0" e1="DrugDDI.d51.s4.e0" e2="DrugDDI.d51.s4.e1" interaction="?"/></sentence></document>