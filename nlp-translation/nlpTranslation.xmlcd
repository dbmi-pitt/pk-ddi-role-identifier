	<sentence id="DBMI.pac181.s32" origId="s32" text="Concomitant administration of R-verapamil can decrease the clearance of paclitaxel.">
		<entity charOffset="4186-4195" id="DBMI.pac181.s32.e0" origId="s32.e0" text="verapamil" type="Active ingredient"/>
