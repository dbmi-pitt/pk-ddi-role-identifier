		<pair e1="DBMI.pac113.sp1.e0" e2="DBMI.pac113.sp1.e1" id="DBMI.pac113.sp1.p0" interaction="true" interactionPhraseType="Quantitative" modality="Positive" object="nifedipine" precipitant="cimetidine"/>
