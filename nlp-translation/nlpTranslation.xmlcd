<document id="DBMI.pac17" origId="pac17">
	<sentence id="DBMI.pac17.s0" origId="s0" text="In a placebo-controlled trial in normal volunteers, administration of sertraline for 22 days (including 200 mg/day for the final 13 days) caused a statistically significant 16% decrease from baseline in the clearance of tolbutamide following an intravenous 1000 mg dose.">
		<entity charOffset="70-80" id="DBMI.pac17.s0.e0" origId="s0.e0" text="sertraline" type="Active ingredient"/>
