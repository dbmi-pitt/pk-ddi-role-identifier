		<pair e1="DBMI.pac139.s5.e2" e2="DBMI.pac139.s5.e1" id="DBMI.pac139.s5.p5" interaction="true" interactionPhraseType="Qualitative" modality="Positive" object="fexofenadine" precipitant="erythromycin"/>
		<pair e1="DBMI.pac139.s5.e2" e2="DBMI.pac139.s5.e1" id="DBMI.pac139.s5.p6" interaction="true" interactionPhraseType="Qualitative" modality="Positive" object="fexofenadine" precipitant="erythromycin"/>
	</sentence>
	<sentence id="DBMI.pac139.s6" origId="s6" text="Fexofenadine had no effect on the pharmacokinetics of either erythromycin or ketoconazole.">
