		<pair e1="DBMI.pac139.s6.e0" e2="DBMI.pac139.s6.e2" id="DBMI.pac139.s6.p1" interaction="true" interactionPhraseType="Qualitative" modality="Negative" object="ketoconazole" precipitant="Fexofenadine"/>
