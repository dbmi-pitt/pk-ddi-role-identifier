		<pair e1="DBMI.pac155.s76.e0" e2="DBMI.pac155.s76.e2" id="DBMI.pac155.s76.p1" interaction="false" interactionPhraseType="" modality="" object="" precipitant=""/>
		<pair e1="DBMI.pac155.s76.e1" e2="DBMI.pac155.s76.e0" id="DBMI.pac155.s76.p2" interaction="false" interactionPhraseType="" modality="" object="" precipitant=""/>
		<pair e1="DBMI.pac155.s76.e1" e2="DBMI.pac155.s76.e2" id="DBMI.pac155.s76.p3" interaction="true" interactionPhraseType="Qualitative" modality="Negative" object="atorvastatin" precipitant="sildenafil"/>
