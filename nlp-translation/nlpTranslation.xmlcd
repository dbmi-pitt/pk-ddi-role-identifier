		<pair e1="DBMI.pac123.s63.e0" e2="DBMI.pac123.s63.e1" id="DBMI.pac123.s63.p1" interaction="true" interactionPhraseType="Qualitative" modality="Negative" object="nifedipine" precipitant="AVANDIA"/>
