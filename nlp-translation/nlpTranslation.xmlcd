		<pair e1="DBMI.pac181.s49.e1" e2="DBMI.pac181.s49.e2" id="DBMI.pac181.s49.p4" interaction="true" interactionPhraseType="Qualitative" modality="Positive" object="lithium" precipitant="verapamil"/>
