		<pair e1="DBMI.pac32.s3.e1" e2="DBMI.pac32.s3.e0" id="DBMI.pac32.s3.p1" interaction="false" interactionPhraseType="" modality="" object="" precipitant=""/>
	</sentence>
	<sentence id="DBMI.pac32.s4" origId="s4" text="On the first day of dosing, fluconazole increased the midazolam AUC and Cmax by 259% and 150%, respectively.">
